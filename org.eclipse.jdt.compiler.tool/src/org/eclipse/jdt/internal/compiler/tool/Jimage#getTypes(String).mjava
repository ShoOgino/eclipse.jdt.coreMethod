	@Override
	public List<String[]> getTypes(String packageName) {
		// package name is expected to ends with '/'
		if (this.packagesCache == null) {
			try {
				this.initialize();
			} catch(IOException e) {
				return Collections.<String[]>emptyList();
			}
		}
		return this.packagesCache.get(packageName);
	}

