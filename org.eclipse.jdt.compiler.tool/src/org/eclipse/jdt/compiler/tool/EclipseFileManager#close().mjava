	/* (non-Javadoc)
	 * @see javax.tools.JavaFileManager#close()
	 */
	public void close() throws IOException {
		this.locations = null;
		for (Archive archive : archivesCache.values()) {
			if (archive.file == null) {
				System.out.println(archive == Archive.UNKNOWN_ARCHIVE);
			}
			archive.close();
		}
	}

