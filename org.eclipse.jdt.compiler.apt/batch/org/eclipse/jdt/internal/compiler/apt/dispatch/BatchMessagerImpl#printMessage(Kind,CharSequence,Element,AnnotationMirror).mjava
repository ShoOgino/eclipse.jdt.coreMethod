	@Override
	public void printMessage(Kind kind, CharSequence msg, Element e,
			AnnotationMirror a) {
		// TODO Auto-generated method stub
		System.err.println("APT says: " + kind + " " + msg + " on element " + e + 
				" at annotation " + a);

	}

