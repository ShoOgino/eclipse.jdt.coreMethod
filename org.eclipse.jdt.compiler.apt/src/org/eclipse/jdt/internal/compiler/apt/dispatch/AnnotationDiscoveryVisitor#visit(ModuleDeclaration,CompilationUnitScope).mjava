	@Override
	public boolean visit(ModuleDeclaration module, CompilationUnitScope scope) {
		ModuleBinding binding = module.binding;
		if (binding == null) {
			return false;
		}
		Annotation[] annotations = module.annotations;
		if (annotations != null) {
			this.resolveAnnotations(
					module.scope,
					annotations,
					binding);
		}
		return true;
	}

