// simulate incremental compile
public void test0972() {
	this.runConformTest(
			new String[] {
				"Outer.java", //================================
				"//Outer.java\n" + 
				"public class Outer<O> {\n" + 
				"  public class Inner {}\n" + 
				"\n" + 
				"  public static void method(Outer<?>.Inner x) {\n" + 
				"    System.out.println(\"SUCCESS\");\n" +
				"  }\n" + 
				"  public static void main(String[] args) {\n" + 
				"    Outer<?>.Inner x = null;\n" +
				"    method(x);\n" +
				"  }\n" + 
				"}\n" + 
				"\n",
				"ExtendedOuter.java", //================================
				"public class ExtendedOuter<E> extends Outer<E> {\n" + 
				"  class ExtendedInner extends Inner {\n" + 
				"    {\n" + 
				"      Outer.method(this);\n" + 
				"    }\n" + 
				"  }\n" + 
				"}\n"
			},
			"SUCCESS");
	this.runConformTest(
			new String[] {
				"Outer.java", //================================
				"//Outer.java\n" + 
				"public class Outer<O> {\n" + 
				"  public class Inner {}\n" + 
				"\n" + 
				"  public static void method(Outer.Inner x) {\n" + 
				"    System.out.println(\"SUCCESS\");\n" +
				"  }\n" + 
				"  public static void main(String[] args) {\n" + 
				"    Outer.Inner x = null;\n" +
				"    method(x);\n" +
				"  }\n" + 
				"}\n" + 
				"\n",
			},
			"SUCCESS",
			null,
			false,
			null);
	this.runConformTest(
			new String[] {
					"ExtendedOuter.java", //================================
					"public class ExtendedOuter<E> extends Outer<E> {\n" + 
					"  class ExtendedInner extends Inner {\n" + 
					"    {\n" + 
					"      Outer.method(this);\n" + 
					"    }\n" + 
					"  }\n" + 
					"  public static void main(String[] args) {\n" + 
					"    System.out.println(\"SUCCESS\");\n" +
					"  }\n" + 
					"}\n"

			},
			"SUCCESS",
			null,
			false,
			null);
}

