	public void testBug50695b() {
		reportMissingJavadocComments = CompilerOptions.IGNORE;
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
					"	/**\n" + 
					"	 * @see java.unknown\n" + 
					"	 */\n" + 
					"	void foo() {}\n" + 
					"}\n"
			 },
			"----------\n" + 
				"1. ERROR in X.java (at line 3)\n" + 
				"	* @see java.unknown\n" + 
				"	       ^^^^^^^^^^^^\n" + 
				"Javadoc: java.unknown cannot be resolved or is not a type\n" + 
				"----------\n"
		);
	}

