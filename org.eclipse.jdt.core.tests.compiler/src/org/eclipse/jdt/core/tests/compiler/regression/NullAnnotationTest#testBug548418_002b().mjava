			"				yield \"hello\";\n" +
			"			}\n" +
			"			default : yield \"world\";\n" +
			"			});\n" +
			"		}\n" +
			"		void m3() {\n" +
			"			Zork();\n" +
			"		}\n" +
			"}\n"
