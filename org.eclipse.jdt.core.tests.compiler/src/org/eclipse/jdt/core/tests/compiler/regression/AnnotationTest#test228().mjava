public void test228() {
	this.runNegativeTest(
		new String[] {
				"X.java",
				"public class X {\n" + 
				"	@SuppressWarnings(\"unchecked\") //unused\n" + 
				"	void doNoEvil(){\n" + 
				"	}\n" + 
				"	Zork z;\n" + 
				"}\n",
		},
		"----------\n" + 
		"1. WARNING in X.java (at line 2)\n" + 
		"	@SuppressWarnings(\"unchecked\") //unused\n" + 
		"	                  ^^^^^^^^^^^\n" + 
		"Unnecessary @SuppressWarnings(\"unchecked\")\n" + 
		"----------\n" + 
		"2. ERROR in X.java (at line 5)\n" + 
		"	Zork z;\n" + 
		"	^^^^\n" + 
		"Zork cannot be resolved to a type\n" + 
		"----------\n");
}

