	// null analysis -- while
	// origin AssignmentTest.testO22
	public void test0129_while_try() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean bool() { return true; }\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"		 while (bool()) {\n" + 
				"			 try {\n" + 
				"				 if (o == null) {\n" + 
				"					 o = new Object();\n" + 
				"				 }\n" + 
				"			 } finally { /* */ }\n" + 
				"		 }\n" + 
				"	 }\n" + 
				"}",
			},
		"");
	}

