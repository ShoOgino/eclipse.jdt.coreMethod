public void test38() {
	Map customOptions = getCompilerOptions();
	customOptions.put(CompilerOptions.OPTION_TaskTags, "TODO:");
	this.runNegativeTest(
		new String[] {
			"X.java",
			"// TODO: something"
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 1)\n" + 
		"	// TODO: something\n" + 
		"	^^^^^^^^^^^^^^^^^^\n" + 
		"Unexpected end of comment\n" + 
		"----------\n",
		null,
		true,
		customOptions);
}

