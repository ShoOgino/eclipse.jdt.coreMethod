	// origin: AssignmentTest#test020
	public void test034_autoboxing() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 void foo() {\n" + 
				"		 int i = 0;\n" +
				"    boolean b = i < 10;\n" + 
				"	 }\n" + 
				"}\n",
			},
		"");
	}

