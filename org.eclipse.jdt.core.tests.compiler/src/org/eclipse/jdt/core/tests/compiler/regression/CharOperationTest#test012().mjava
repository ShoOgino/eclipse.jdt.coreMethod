// test the javadoc examples
public void test012() {
	assertTrue("Should match", CharOperation.match(
			new char[] { '?', 'b', '*' },
			1,
			3,
			new char[] { 'a', 'b', 'c' , 'd' },
			1,
			4,
			true));
	assertFalse("Should not match", CharOperation.match(
			new char[] { '?', 'b', '*' },
			1,
			2,
			new char[] { 'a', 'b', 'c' , 'd' },
			1,
			4,
			true));
}

