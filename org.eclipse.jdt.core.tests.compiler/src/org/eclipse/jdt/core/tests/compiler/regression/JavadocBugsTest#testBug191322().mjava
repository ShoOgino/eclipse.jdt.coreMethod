	/**
	 * @bug 191322: [javadoc] @see or @link reference to method without signature fails to resolve to base class method
	 * @see "https://bugs.eclipse.org/bugs/show_bug.cgi?id=191322"
	 */
	public void testBug191322() {
		runConformTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	void foo() {}\n" + 
				"	/**\n" + 
				"	 * {@link #foo}.\n" + 
				"	 * @see #foo\n" + 
				"	 */\n" + 
				"	void goo() {}\n" + 
				"}\n",
				"Y.java",
				"class Y extends X {\n" + 
				"	/**\n" + 
				"	 * {@link #foo}\n" + 
				"	 * @see #foo\n" + 
				"	 */\n" + 
				"	void hoo() {}\n" + 
				"}"
			}
		);
	}

