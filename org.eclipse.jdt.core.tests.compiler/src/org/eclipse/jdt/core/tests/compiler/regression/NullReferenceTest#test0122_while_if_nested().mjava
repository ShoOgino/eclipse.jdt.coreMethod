	// null analysis -- while
	public void test0122_while_if_nested() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean dummy, other;\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"		 while (dummy) {\n" +   
				"		   if (other) {\n" + 
				"		     o.toString();\n" +    
				"		   }\n" + 
				"		   o = new Object();\n" + 
				"		 }\n" + 
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

