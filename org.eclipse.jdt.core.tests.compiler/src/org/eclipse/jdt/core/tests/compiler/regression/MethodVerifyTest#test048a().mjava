	public void test048a() {
		String expectedCompilerLog = (this.complianceLevel == ClassFileConstants.JDK1_6)?
				"----------\n" + 
				"1. WARNING in X2.java (at line 2)\n" + 
				"	public Object foo(I<String> z) { return null; }\n" + 
