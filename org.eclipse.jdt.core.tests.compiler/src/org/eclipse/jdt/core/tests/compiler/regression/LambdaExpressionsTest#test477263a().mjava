			"			}\n" + 
			"			private Y(int y) {\n" + 
			"				System.out.println(y);\n" + 
			"			}\n" + 
			"			 Y() {\n" + 
			"			}\n" + 
			"		}\n" + 
			"		I i = Y :: new;\n" + 
			"		i.makeX(local);\n" + 
			"	}\n" + 
			"	private X(int x) {\n" + 
			"	}\n" + 
			"	X() {\n" + 
			"	}\n" + 
			"	public static void main(String[] args) {\n" + 
