// null analysis - try/catch
// https://bugs.eclipse.org/bugs/show_bug.cgi?id=150854
public void test0564_try_catch_unchecked_exception() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  public static Object foo() {\n" + 
			"    Object result = null;\n" + 
			"    try {\n" + 
			"      result = new Object();\n" + 
			"    } catch (Exception e) {\n" + 
			"      result = null;\n" + 
			"    }\n" + 
			"    return result;\n" + 
			"  }\n" + 
			"}"},
		"");
}

