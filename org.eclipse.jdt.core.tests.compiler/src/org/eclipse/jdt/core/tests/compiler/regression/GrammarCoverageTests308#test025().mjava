		this.runNegativeTest(
				new String[] {
					"X.java",
					"public class X<P, C> {\n" +
					"  public X() {\n" +
					"    if (!(this instanceof @Marker X)) {}\n" +
					"  }\n" +
					"}\n"
				},
				"----------\n" + 
				"1. ERROR in X.java (at line 3)\n" + 
				"	if (!(this instanceof @Marker X)) {}\n" + 
				"	                       ^^^^^^\n" + 
				"Marker cannot be resolved to a type\n" + 
				"----------\n");
	}	

