	// null analysis - if/else
	public void test0105_if_else() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 void foo(Object o) {\n" + 
				"		 if (o == null) {\n" + 
				"			 // do nothing\n" + 
				"		 }\n" + 
				"		 o.toString();\n" + 
				"	 }\n" + 
				"}\n"},
			""
		);
	}

