public void test065() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"	void foo() {\n" + 
			"		try {\n" + 
			"			System.out.println(\"Hello\");\n" + 
			"		} finally {\n" + 
			"			if (true)\n" + 
			"				return;\n" + 
			"		}\n" + 
			"		return;\n" + 
			"	}\n" + 
			"	void bar() {\n" + 
			"		try {\n" + 
			"			System.out.println(\"Hello\");\n" + 
			"		} finally {\n" + 
			"			return;\n" + 
			"		}\n" + 
			"		return;\n" + 
			"	}\n" + 
			"}\n",
		},
		"----------\n" + 
		"1. WARNING in X.java (at line 14)\n" + 
		"	} finally {\n" + 
		"			return;\n" + 
		"		}\n" + 
		"	          ^^^^^^^^^^^^^^^^\n" + 
		"finally block does not complete normally\n" + 
		"----------\n" + 
		"2. ERROR in X.java (at line 17)\n" + 
		"	return;\n" + 
		"	^^^^^^^\n" + 
		"Unreachable code\n" + 
		"----------\n");
}

