	public void testAssertAsAnIdentifier() {
		this.runNegativeTest(
			new String[] {
				"assert.java",
				"public class assert {}\n",
			},
			"----------\n" + 
			"1. ERROR in assert.java (at line 1)\n" + 
			"	public class assert {}\n" + 
			"	             ^^^^^^\n" + 
			"Syntax error on keyword \"assert\"; \"Identifier\" expected\n" + 
			"----------\n");
	}

