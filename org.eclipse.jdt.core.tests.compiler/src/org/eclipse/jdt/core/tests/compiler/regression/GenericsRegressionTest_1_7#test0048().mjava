	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X<T> {\n" +
			"    <T> X(T t) {\n" +
			"    }\n" +
			"    X<String> x = new X<>(\"\"); \n" +
			"    Zork z;\n" +
			"}\n"
		},
		"----------\n" + 
		"1. WARNING in X.java (at line 2)\n" + 
		"	<T> X(T t) {\n" + 
		"	 ^\n" + 
