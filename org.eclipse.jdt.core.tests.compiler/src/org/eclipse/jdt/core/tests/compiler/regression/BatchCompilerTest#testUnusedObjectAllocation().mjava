public void testUnusedObjectAllocation() {
	runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" +
			"	void foo() {\n" +
			"		new X();\n" +
			"	}\n" +
			"}\n"
		},
		"\"" + OUTPUT_DIR +  File.separator + "X.java\""
		+ " -err:+unused"
		+ " -d \"" + OUTPUT_DIR + File.separator + "bin/\"",
		"",
		"----------\n" + 
		"1. ERROR in ---OUTPUT_DIR_PLACEHOLDER---/X.java (at line 3)\n" + 
		"	new X();\n" + 
		"	^^^^^^^\n" + 
		"The allocated object is never used\n" + 
		"----------\n" + 
		"1 problem (1 error)\n",
		true);

}

