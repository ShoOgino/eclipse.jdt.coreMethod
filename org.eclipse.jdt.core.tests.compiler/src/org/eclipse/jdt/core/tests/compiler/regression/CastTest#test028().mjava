/*
 * https://bugs.eclipse.org/bugs/show_bug.cgi?id=47074
 */
public void test028() { 
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  public static void main(String[] args) {\n" + 
			"    A a = null;\n" + 
			"    B b = null;\n" + 
			"    boolean c = a == b;\n" + 
			"  }\n" + 
			"}\n" + 
			"interface A {\n" + 
			"  void doSomething();\n" + 
			"}\n" + 
			"interface B {\n" + 
			"  int doSomething();\n" + 
			"}",
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 5)\n" + 
		"	boolean c = a == b;\n" + 
		"	            ^^^^^^\n" + 
		"Incompatible operand types A and B\n" + 
		"----------\n",
		null,
		true);
}

