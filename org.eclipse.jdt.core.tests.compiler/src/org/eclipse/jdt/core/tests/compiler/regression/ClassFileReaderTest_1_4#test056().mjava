	public void test056() throws Exception {
		String source =
			"public class X {\n" +
			"	static boolean boom() { \n" +
			"		throw new NullPointerException();\n" +
			"	}\n" +
			"	public static void main(String[] args) {\n" +
			"		bar2();\n" +
			"	}\n" +
			"	static void bar2() {\n" +
			"		int i = 6;\n" +
			"		if ((i == 6) || (boom() || true)) {\n" +
			"			System.out.println(i);\n" +
			"		}\n" +
			"	}\n" +
			"}";
		String expectedOutput =
			"  // Method descriptor #6 ()V\n" +
			"  // Stack: 2, Locals: 1\n" +
			"  static void bar2();\n" +
			"     0  bipush 6\n" +
			"     2  istore_0 [i]\n" +
			"     3  iload_0 [i]\n" +
			"     4  bipush 6\n" +
			"     6  if_icmpeq 13\n" +
			"     9  invokestatic X.boom() : boolean [26]\n" +
			"    12  pop\n" +
			"    13  getstatic java.lang.System.out : java.io.PrintStream [28]\n" +
			"    16  iload_0 [i]\n" +
			"    17  invokevirtual java.io.PrintStream.println(int) : void [34]\n" +
			"    20  return\n" +
			"      Line numbers:\n" +
			"        [pc: 0, line: 9]\n" +
			"        [pc: 3, line: 10]\n" +
			"        [pc: 13, line: 11]\n" +
			"        [pc: 20, line: 13]\n" +
			"      Local variable table:\n" +
			"        [pc: 3, pc: 21] local: i index: 0 type: int\n";
		checkClassFile("X", source, expectedOutput);
	}

