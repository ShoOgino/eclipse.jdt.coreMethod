// null analysis -- potentially redundant checks against the same field
public void testBug247564l_1() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" +
			"    private Object f;\n" +
			"	 int foo() throws Exception{\n" +
			"		 if (f == null && f != null)\n" +
			"            return 13;\n" +
			"        return -13;\n" +
			"    }\n" +
			"	 int goo() throws Exception{\n" +
			"		 if (f == null && f == null)\n" +
			"            return 14;\n" +
			"        return -14;\n" +
			"    }\n" +
			"	 boolean hoo() throws Exception{\n" +
			"		 if (f == null)\n" +
			"            return f != null;\n" +
			"        return f == null;\n" +
			"    }\n" +
			"}\n"},
		""
	);
}

