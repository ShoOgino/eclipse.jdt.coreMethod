	public void test066() {
		String source =
				"public class X {\n" + 
				"	double d = 0X.p02d;\n" + 
				"}";
		if (this.complianceLevel > ClassFileConstants.JDK1_4) {
			this.runNegativeTest(
					new String[] {
							"X.java",
							source
					},
					"----------\n" + 
							"1. ERROR in X.java (at line 2)\n" + 
							"	double d = 0X.p02d;\n" + 
							"	           ^^^\n" + 
							"Invalid hex literal number\n" + 
					"----------\n");
		}
	}

