	public void testBug129241b() {
		this.reportDeprecation = CompilerOptions.IGNORE;
		runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" +
				"	/**\n" +
				"	 * @see p.A#bar\n" +
				"	 */\n" +
				"	void foo() {}\n" +
				"}\n",
				"p/A.java",
				"package p;\n" +
				"/** @deprecated */\n" +
				"public class A {\n" +
				"	void bar() {}\n" +
				"}\n"
			},
			"----------\n" +
			"1. ERROR in X.java (at line 3)\n" +
			"	* @see p.A#bar\n" +
			"	       ^^^\n" +
			"Javadoc: The type A is deprecated\n" +
			"----------\n",
			JavacTestOptions.Excuse.EclipseWarningConfiguredAsError
		);
	}

