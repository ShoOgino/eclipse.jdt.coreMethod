// check enum constants with extra arguments
public void test014() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public enum X { \n" + 
			"	\n" + 
			"	BLEU(10),\n" + 
			"	BLANC(20),\n" + 
			"	ROUGE(30);\n" + 
			"\n" + 
			"	int val;\n" + 
			"	X(int val) {\n" + 
			"		this.val = val;\n" + 
			"	}\n" + 
			"\n" + 
			"	public static void main(String[] args) {\n" + 
			"		for(X x: values()) {\n" + 
			"			System.out.print(x.val);\n" + 
			"		}\n" + 
			"	}\n" + 
			"}\n"
		},
		"102030");
}	

