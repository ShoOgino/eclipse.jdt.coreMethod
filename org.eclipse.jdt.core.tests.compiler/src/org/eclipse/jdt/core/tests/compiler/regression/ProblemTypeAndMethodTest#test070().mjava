public void test070() {
	this.runNegativeTest(
			new String[] {
				"p/X.java", //-----------------------------------------------------------------------
				"package p;\n" +
				"public class X {\n" + 
				"	IOException foo() {}\n" +
				"}\n",
				"q/Y.java", //-----------------------------------------------------------------------
				"package q;\n" +
				"import p.*;\n" + 
				"import java.io.*;\n" + 
				"public class Y {\n" + 
				"   void foo(IOException e) {}\n" +
				"   void bar(Zork z) {}\n" +
				"}\n"
			},
			"----------\n" + 
			"1. ERROR in p\\X.java (at line 3)\n" + 
			"	IOException foo() {}\n" + 
			"	^^^^^^^^^^^\n" + 
			"IOException cannot be resolved to a type\n" + 
			"----------\n" + 
			"----------\n" + 
			"1. ERROR in q\\Y.java (at line 6)\n" + 
			"	void bar(Zork z) {}\n" + 
			"	         ^^^^\n" + 
			"Zork cannot be resolved to a type\n" + 
			"----------\n");
}

