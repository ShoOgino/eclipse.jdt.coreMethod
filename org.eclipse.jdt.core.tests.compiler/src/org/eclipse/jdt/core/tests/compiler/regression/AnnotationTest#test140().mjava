    // https://bugs.eclipse.org/bugs/show_bug.cgi?id=90111 - variation
    public void test140() {
        this.runNegativeTest(
            new String[] {
                "X.java",
				"public class X {\n" + 
				"  static void foo(){}\n" + 
				"}\n" + 
				"class Bar extends X {\n" + 
				"  @Override\n" + 
				"  static void foo(){}	\n" + 
				"}\n" + 
				"\n"
            },
    		"----------\n" + 
    		"1. ERROR in X.java (at line 6)\r\n" + 
    		"	static void foo(){}	\r\n" + 
    		"	            ^^^^^\n" + 
    		"The method foo() of type Bar must override a superclass method\n" + 
    		"----------\n");
    }              

