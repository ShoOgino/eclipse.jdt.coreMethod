			"		final boolean TRUE = true;\n" + 
			"		goo((x) -> {do {throw new Exception();}while (FALSE);});\n" + 
			"		goo((x) -> {do { return \"\";}while (false);});\n" + 
			"		goo((x) -> {do {\n" + 
