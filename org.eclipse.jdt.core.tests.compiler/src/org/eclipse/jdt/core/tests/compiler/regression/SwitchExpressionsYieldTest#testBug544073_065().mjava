	public void testBug544073_065() {
		runConformTest(
			new String[] {
				"X.java",
				"public class X {\n" +
				"	public int foo(int i) {\n" +
				"		@SuppressWarnings(\"preview\")\n" +
				"		int v =\n" +
				"			switch(switch(i) {\n" +
				"					case 0 -> { yield 2; }\n" +
				"					default -> { yield 3; }\n" +
