				"			case 0 -> { yield 0; }\n" +
				"			default -> { yield 1; }\n" +
				"		};\n" +
				"	return v == 1 ? v : 0;\n" +
				"	}\n" +
				"	public static void main(String[] argv) {\n" +
				"		System.out.println(new X().foo(0));\n" +
				"	}\n" +
				"}"
		},
		"1");
	}

