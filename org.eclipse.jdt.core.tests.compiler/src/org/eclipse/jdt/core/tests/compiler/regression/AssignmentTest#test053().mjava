// https://bugs.eclipse.org/bugs/show_bug.cgi?id=206017
public void test053() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  int i = \"aaa\";\n" + 
			"  { \n" + 
			"    i = \"bbb\";\n" + 
			"  }\n" + 
			"}"
			},
			"----------\n" + 
			"1. ERROR in X.java (at line 2)\n" + 
			"	int i = \"aaa\";\n" + 
			"	        ^^^^^\n" + 
			"Type mismatch: cannot convert from String to int\n" + 
			"----------\n" + 
			"2. ERROR in X.java (at line 4)\n" + 
			"	i = \"bbb\";\n" + 
			"	    ^^^^^\n" + 
			"Type mismatch: cannot convert from String to int\n" + 
			"----------\n");
}

