public void testDo2() {
	this.runConformTest(
		new String[] {
			"X.java",
			"interface I {\n" +
			"	int foo();\n" +
			"}\n" +
			"public class X {\n" +
			"   static void goo(I i) {\n" +
			"		System.out.println(\"goo(I)\");\n" +
			"   }\n" +
			"	public static void main(String[] args) {\n" +
			"		goo(() -> {\n" +
			"			final boolean t = true;\n" +
			"			do {\n" +
			"				System.out.println();\n" +
			"				throw new RuntimeException();\n" +
			"			} while (t);\n" +
			"		});\n" +
			"	}\n" +
			"}\n"
		},
		"goo(I)");
}

