	// null analysis -- while
	public void test0126_while() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean dummy;\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"		 while (dummy) {\n" + 
				"		   o = new Object();\n" + 
				"		 }\n" + 
				"		 o.toString();\n" + 
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

