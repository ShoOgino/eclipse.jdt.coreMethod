			"  String foo(Number n);\n" + 
			"}\n" + 
			"interface K {\n" + 
			"  Object foo(Number n);\n" + 
			"}\n" + 
			"public abstract class X implements J, K {\n" + 
			"  void foo() {\n" + 
			"    foo(0.0f);\n" + 
			"  }\n" + 
			"}"
		},
		"");
