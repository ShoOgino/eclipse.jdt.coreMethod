	// null analysis -- switch
	public void test0303_switch() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	int k;\n" + 
				"	void foo() {\n" + 
				"		Object o = null;\n" + 
				"		switch (k) {\n" + 
				"			case 0 :\n" + 
				"			  o = new Object();\n" + 
				"			case 1 :\n" + 
				"				o.toString();\n" + // quiet: may come through 0 or 1
				"				break;\n" + 
				"		}\n" + 
				"	}\n" + 
				"}\n",
			},
		"");
	}

