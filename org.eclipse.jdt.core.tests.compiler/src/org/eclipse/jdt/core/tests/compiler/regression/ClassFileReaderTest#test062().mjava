			"		foo4();\n" + 
			"	}\n" + 
			"	static void foo4() {\n" + 
			"		int i = 5;\n" + 
			"		if ((false && boom()) && (i == 6)) {\n" + 
			"		} else {  	\n" + 
			"		   System.out.println(i);\n" + 
			"		}\n" + 
			"	}\n" + 
			"}";
		String expectedOutput =
