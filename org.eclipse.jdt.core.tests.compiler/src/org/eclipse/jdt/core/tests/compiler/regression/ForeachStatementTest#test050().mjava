public void test050() throws Exception {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X<T> {\n" + 
			"	private T values;\n" + 
			"	public X(T values) {\n" + 
			"		this.values = values;\n" + 
			"	}\n" + 
			"	public T getValues() {\n" + 
			"		return values;\n" + 
			"	}\n" + 
			"	public static void main(String[] args) {\n" +
			"		X<short[]> x = new X<short[]>(new short[] { 1, 2, 3, 4, 5 });\n" + 
			"		for (int i : x.getValues()) {\n" + 
			"			System.out.print(i);\n" + 
			"		}\n" + 
			"	}\n" +
			"}",
		},
		"12345");
}

