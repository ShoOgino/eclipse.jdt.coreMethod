	public void test048() {
		this.runNegativeTest(
				new String[] {
					"X.java",
					"public class X {\n" + 
					"        Y<String> [] foo() {\n" + 
					"                return null;\n" + 
					"        }\n" + 
					"        void bar(Y... y) {\n" + 
					"        }\n" + 
					"        void fred() {\n" + 
					"                bar(foo());\n" + 
					"                bar((Y[])foo());\n" + 
					"                Zork z;\n" +
					"        }\n" + 
					"}\n" + 
					"class Y<E> {\n" + 
					"}\n",
				},
				"----------\n" + 
				"1. WARNING in X.java (at line 9)\n" + 
				"	bar((Y[])foo());\n" + 
				"	    ^^^^^^^^^^\n" + 
				"Unnecessary cast from Y<String>[] to Y[]\n" + 
				"----------\n" + 
				"2. ERROR in X.java (at line 10)\n" + 
				"	Zork z;\n" + 
				"	^^^^\n" + 
				"Zork cannot be resolved to a type\n" + 
				"----------\n");
	}

