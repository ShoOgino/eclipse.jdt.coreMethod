	// null analysis -- while
	public void test0121_while_nested() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean dummy;\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"		 while (dummy) {\n" + 
				"		   while (o != null) {\n" + 
				"		     o.toString();\n" + 
				"		   }\n" + 
				"		   if (System.currentTimeMillis() > 10L) {\n" + 
				"		     o = new Object();\n" + 
				"		   }\n" + 
				"		 }\n" + 
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

