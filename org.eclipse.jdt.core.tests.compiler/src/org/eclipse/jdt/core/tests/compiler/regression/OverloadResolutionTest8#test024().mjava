public void test024() {
	this.runNegativeTest(
			new String[] {
				"X.java",
				"interface I {\n" +
				"	Y foo(int x);\n" +
				"}\n" +
				"interface J {\n" +
				"	Y foo(int x);\n" +
				"}\n" +
				"class Y {\n" +
				"	Y(int x) {\n" +
				"	}\n" +
				"	\n" +
				"}\n" +
				"public class X {\n" +
				"	static void foo(I i) {\n" +
				"	}\n" +
				"	static void foo(J j) {\n" +
				"	}\n" +
				"	public static void main(String[] args) {\n" +
				"		foo(Y::new);\n" +
				"	}\n" +
				"}\n",
			},
			"----------\n" + 
			"1. ERROR in X.java (at line 18)\n" + 
			"	foo(Y::new);\n" + 
			"	^^^\n" + 
			"The method foo(I) is ambiguous for the type X\n" + 
			"----------\n");
}

