// Bug 345305 - [compiler][null] Compiler misidentifies a case of "variable can only be null"
// assignment in method argument position
public void testBug345305_3() {
	runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" +
			"    void foo() {\n" +
			"        String s = null;\n" + 
			"        while (true) {\n" + 
			"            try {\n" + 
			"                check(s = \"hi\");\n" + 
			"            }\n" + 
			"            finally {\n" + 
			"                s.length();\n" + 
			"                s = null;\n" + 
			"            }\n" + 
			"        }\n" + 
			"    }\n" +
			"    void check(String s) {}\n" +
			"}\n"
		});
}

