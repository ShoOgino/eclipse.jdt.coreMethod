public void test1177() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X<T extends Foo & Bar> {\n" + 
			"	T get() { return null; }\n" + 
			"	void method(X<? extends Runnable> x1, X<? extends java.io.Serializable> x2, boolean b) {\n" + 
			"		(b ? x1 : x2).get().foo();\n" + 
			"		(b ? x1 : x2).get().bar();\n" + 
			"	}\n" + 
			"}\n" + 
			"class Foo {\n" + 
			"	void foo() {/**/}\n" + 
			"}\n" + 
			"interface Bar {\n" + 
			"	void bar();\n" + 
			"}\n" + 
			"\n", // =================
		},
		"");
}

