	// null analysis -- while
	public void test0126_while_unknown_if_else() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean dummy;\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" +
				"		 if (dummy) {\n" +
				"		   o = new Object();\n" +
				"		 }\n" +
				"		 while (dummy) {\n" +
				"		   if (o == null) {/* */}\n" +
				"		 }\n" +
				"		 o.toString();\n" +
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

