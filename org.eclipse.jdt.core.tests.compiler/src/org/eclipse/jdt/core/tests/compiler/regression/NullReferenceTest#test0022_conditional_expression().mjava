	// null analysis -- conditional expression
	public void test0022_conditional_expression() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 void foo() {\n" + 
				"		 Object o = false ? null : new Object();\n" + 
				"		 o.toString();\n" + 
				"	 }\n" + 
				"}\n"},
			""
		);
	}

