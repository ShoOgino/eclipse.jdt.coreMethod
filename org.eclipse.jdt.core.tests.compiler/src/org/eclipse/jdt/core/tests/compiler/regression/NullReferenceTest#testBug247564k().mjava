// null analysis -- simple case for field in try-finally
public void testBug247564k() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" +
			"  private Object f;\n" +
			"	 void goo(Object var) {\n" +
			"		try {\n" +
			"			int i = 10;\n" +
			"			while (i<20){\n" +
			"				if (i == 15) {\n" +
			"					f = null;\n" +
			"					break;\n" +
			"				}\n" +
			"				i++;\n" +
			"			}\n" +
			"			return;\n" +
			"		} finally {\n" +
			"			if (f != null && f.hashCode() == 0){}\n" +
			"		}\n" +
			"  }\n" +
			"}\n"},
		""
	);
}

