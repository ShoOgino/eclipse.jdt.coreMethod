// check enum constants with wrong arguments
public void test015() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public enum X { \n" + 
			"	\n" + 
			"	BLEU(10),\n" + 
			"	BLANC(),\n" + 
			"	ROUGE(30);\n" + 
			"\n" + 
			"	int val;\n" + 
			"	X(int val) {\n" + 
			"		this.val = val;\n" + 
			"	}\n" + 
			"\n" + 
			"	public static void main(String[] args) {\n" + 
			"		for(X x: values()) {\n" + 
			"			System.out.print(x.val);\n" + 
			"		}\n" + 
			"	}\n" + 
			"}\n"
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 4)\n" + 
		"	BLANC(),\n" + 
		"	^^^^^\n" + 
		"The constructor X() is undefined\n" + 
		"----------\n");
}		

