public void test934() {
	this.runNegativeTest(
		new String[] {
		"X.java", //================================
		"public class X<T> {\n" + 
		"	static class Inner<U> {\n" + 
		"		static class InInner <V> {\n" + 
		"		}\n" + 
		"	}\n" + 
		"}\n" + 
		"class Y<W> extends X<W> {\n" + 
		"	void foo() {\n" + 
		"		Inner<W> inner = null;\n" + 
		"		String s = inner;\n" + 
		"	}\n" + 
		"}\n",
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 10)\n" + 
		"	String s = inner;\n" + 
		"	           ^^^^^\n" + 
		"Type mismatch: cannot convert from X.Inner<W> to String\n" + 
		"----------\n");
}

