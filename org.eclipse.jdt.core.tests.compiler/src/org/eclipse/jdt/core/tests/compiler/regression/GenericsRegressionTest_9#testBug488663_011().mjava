// anonymous diamond with polytype argument
public void testBug488663_011() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"	String name;\n" + 
			"	public X(String name) {\n" + 
			"		this.name = name;\n" + 
			"	}\n" + 
			"	public static void main(String[] args) {\n" + 
			"		Y<String> y = new Y<>(Y::foo) {\n" + 
			"		};\n" + 
			"	}\n" + 
			"}\n" + 
			"interface J {\n" + 
			"	void doSomething();\n" + 
			"}\n" + 
			"class Y<T> {\n" + 
			"	public Y(J j) {\n" + 
			"		j.doSomething();\n" + 
			"	}\n" + 
			"	static void foo() {\n" + 
			"		System.out.println(\"Done\");\n" + 
			"	}\n" +
			"}",
		},
		"Done");
}

