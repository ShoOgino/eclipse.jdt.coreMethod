		"class A {\n" + 
		"  A() {\n" + 
		"    super();\n" + 
		"  }\n" + 
		"  public <Integer, @Positive Integer, @Negative Integer, Integer>@Marker int foo() {\n" + 
		"    return 0;\n" +
		"  }\n" + 
		"  public <Integer, @Positive Integer, @Negative Integer, Integer>int bar() {\n" + 
		"    return 0;\n" +
		"  }\n" + 
		"}\n";
