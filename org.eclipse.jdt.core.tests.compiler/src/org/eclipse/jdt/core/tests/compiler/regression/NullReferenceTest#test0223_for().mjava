	// null analysis -- for
	public void test0223_for() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"		 for (;o == null;) {\n" + 
				      // quiet: first iteration is sure to find it null, 
				      // but other iterations may change it 
				"		   o = new Object();\n" + 
				"		 }\n" + 
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

