	// null analysis -- field
	public void test0004_field_with_method_call() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 Object o;\n" + 
				"	 void foo() {\n" + 
				"		 o = null;\n" + 
				"		 bar();\n" + // defuses null by side effect
				"		 o.toString();\n" + 
				"	 }\n" + 
				"	 void bar() {\n" + 
				"	 }\n" + 
				"}\n"},
			""
		);
	}

