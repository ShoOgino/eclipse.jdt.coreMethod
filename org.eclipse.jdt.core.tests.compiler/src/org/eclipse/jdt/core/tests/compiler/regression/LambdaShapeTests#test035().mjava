public void test035() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"interface I { \n" + 
			"	String foo(int x) throws Exception; \n" + 
			"}\n" + 
			"public class X {\n" + 
			"	void goo(I i) {}\n" + 
			"	void test(String[] strs) {\n" + 
			"		goo((x) -> {\n" + 
			"			for(String str : strs){\n" + 
			"				return \"no\";\n" + 
			"			}\n" + 
			"		});\n" + 
			"	}\n" + 
			"}\n"
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 7)\n" + 
		"	goo((x) -> {\n" + 
		"	    ^^^^^^\n" + 
		"This method must return a result of type String\n" + 
		"----------\n");
}

