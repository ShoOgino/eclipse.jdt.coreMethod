// check diagnosis for duplicate enum constants
public void test002() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public enum X { \n" + 
			"	\n" + 
			"	BLEU, \n" + 
			"	BLANC, \n" + 
			"	ROUGE,\n" + 
			"	BLEU;\n" + 
			"}"
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 3)\n" + 
		"	BLEU, \n" + 
		"	^^^^\n" + 
		"Duplicate field X.BLEU\n" + 
		"----------\n" + 
		"2. ERROR in X.java (at line 6)\n" + 
		"	BLEU;\n" + 
		"	^^^^\n" + 
		"Duplicate field X.BLEU\n" + 
		"----------\n");
}

