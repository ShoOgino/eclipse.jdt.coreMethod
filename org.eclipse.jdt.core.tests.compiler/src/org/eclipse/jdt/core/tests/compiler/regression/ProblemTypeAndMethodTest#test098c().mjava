public void test098c() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" +
			"    private class A {\n" +
			"        public void foo() {}\n" +
			"    }\n" +
			"    public class B extends A {}\n" +
			"}"
		},
		"----------\n" + 
		"1. WARNING in X.java (at line 5)\n" + 
		"	public class B extends A {}\n" + 
		"	             ^\n" + 
		"Access to enclosing constructor X.A() is emulated by a synthetic accessor method\n" + 
		"----------\n");
}

