	// null analysis -- do while
	public void test0204_do_while() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"		 do {\n" + 
				"		   if (System.currentTimeMillis() > 10L) {\n" + 
				"		     o = new Object();\n" + 
				"		   }\n" + 
				"		 }\n" + 
				"		 while (o == null);\n" + 
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

