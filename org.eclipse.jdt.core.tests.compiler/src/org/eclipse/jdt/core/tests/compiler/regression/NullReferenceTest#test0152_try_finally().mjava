	// null analysis -- try/finally
	public void test0152_try_finally() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 Object m;\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"		 try {\n" + 
				"		   System.out.println(); // might throw a runtime exception\n" + 
				"			 o = m;\n" + 
				"		 }\n" + 
				"		 finally {}\n" + 
				"		 o.toString();\n" + 
						// still OK because in case of exception this code is 
						// not reached
				"	 }\n" + 
				"}\n"},
			""
		);
	}

