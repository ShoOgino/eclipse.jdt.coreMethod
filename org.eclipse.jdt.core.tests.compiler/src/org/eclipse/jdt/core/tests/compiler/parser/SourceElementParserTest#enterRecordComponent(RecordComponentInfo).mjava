public void enterRecordComponent(RecordComponentInfo compInfo) {
	this.currentType.addRecordComponent(
		this.currentRecordComp =
			new SourceField(
					compInfo.declarationStart,
					compInfo.modifiers,
					compInfo.type,
					compInfo.name,
					compInfo.nameSourceStart,
					compInfo.nameSourceEnd,
					this.source));

}

