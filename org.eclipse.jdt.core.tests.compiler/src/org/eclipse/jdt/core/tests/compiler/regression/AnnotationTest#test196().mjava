public void test196() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public @interface X {\n" + 
			"    enum MyEnum {\n" + 
			"        VAL_1, VAL_2\n" + 
			"    }\n" + 
			"    public MyEnum theValue() default null;\n" + 
			"}"
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 5)\n" + 
		"	public MyEnum theValue() default null;\n" + 
		"	                                 ^^^^\n" + 
		"The value for annotation attribute X.theValue must be an enum constant expression\n" + 
		"----------\n");
}

