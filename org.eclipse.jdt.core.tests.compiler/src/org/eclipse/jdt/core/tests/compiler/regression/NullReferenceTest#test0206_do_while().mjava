	// null analysis -- do while
	public void test0206_do_while() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null,\n" + 
				"		        u = new Object(),\n" + 
				"		        v = new Object();\n" + 
				"		 do {\n" +
				"		   if (v == null) {\n" +
				"		     o = new Object();\n" +
				"		   };\n" +
				"		   if (u == null) {\n" +
				"		     v = null;\n" +
				"		   };\n" +
				"		   u = null;\n" +
				"		 }\n" +
				"		 while (o == null);\n" +
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

