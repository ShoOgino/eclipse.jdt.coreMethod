	// null analysis - try/catch
	public void test0171_try_catch() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean dummy;\n" + 
				"	 void foo() {\n" + 
				"		 Object o = new Object();\n" + 
				"		 try {\n" + 
				"			 System.out.println();\n" + 
				"			 if (dummy) {\n" + 
				"			   o = null;\n" + 
				"			   throw new Exception();\n" + 
				"			 }\n" + 
				"		 }\n" + 
				"		 catch (Exception e) {\n" + 
				"			 o.toString();\n" +
				 	// quiet: println may throw a RuntimeException
				"		 }\n" + 
				"	 }\n" + 
				"}\n"},
			""
		);
	}

