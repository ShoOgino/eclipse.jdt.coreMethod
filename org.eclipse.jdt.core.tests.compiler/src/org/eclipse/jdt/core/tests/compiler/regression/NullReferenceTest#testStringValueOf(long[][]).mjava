String testStringValueOf(long[][] data) {
	StringBuffer result = new StringBuffer(25);
	result.append('{');
	for (int i = 0; i < 3; i++) {
		if (i > 0) {
			result.append(',');
		}
		result.append('{');
		for (int j = 0; j < 4; j++) {
			if (j > 0) {
				result.append(',');
			}
			result.append(data[i][j]);
		}
		result.append('}');
	}
	result.append('}');
	return result.toString();
}

