	// null analysis -- field
	public void test0010_field_of_another_object() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 Object o;\n" + 
				"	 void foo() {\n" + 
				"	   X other = new X();\n" + 
				"		 other.o = null;\n" + 
				"		 other.o.toString();\n" + 
				"	 }\n" + 
				"}\n"},
			""
		);
	}

