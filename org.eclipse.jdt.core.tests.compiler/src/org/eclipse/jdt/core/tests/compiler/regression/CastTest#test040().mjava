public void test040() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"	{\n" + 
			"		int i = 12;\n" + 
			"		int j = (byte) i;\n" + 
			"		float f = (float) i;\n" + 
			"		Zork z;\n" + 
			"	}\n" + 
			"}\n"
		},
		"----------\n" + 
		"1. WARNING in X.java (at line 5)\n" + 
		"	float f = (float) i;\n" + 
		"	          ^^^^^^^^^\n" + 
		"Unnecessary cast from int to float\n" + 
		"----------\n" + 
		"2. ERROR in X.java (at line 6)\n" + 
		"	Zork z;\n" + 
		"	^^^^\n" + 
		"Zork cannot be resolved to a type\n" + 
		"----------\n");
}

