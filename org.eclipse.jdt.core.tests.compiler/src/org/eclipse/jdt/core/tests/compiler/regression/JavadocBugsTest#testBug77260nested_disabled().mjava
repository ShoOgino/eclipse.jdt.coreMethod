	public void testBug77260nested_disabled() {
		Map options = getCompilerOptions();
		options.put(CompilerOptions.OPTION_ReportDeprecationInDeprecatedCode, CompilerOptions.ENABLED);
		options.put(CompilerOptions.OPTION_ReportInvalidJavadocTagsDeprecatedRef, CompilerOptions.DISABLED);
		runConformTest(
			new String[] {
				"X.java",
				"/** @deprecated */\n" + 
					"public class X {\n" + 
					"	public int x;\n" + 
					"	public void foo() {}\n" + 
					"}\n",
				"Y.java",
				"/**\n" + 
					" * @see X\n" + 
					" * @deprecated\n" + 
					" */\n" + 
					"public class Y {\n" + 
					"	/** @see X#x */\n" + 
					"	public int x;\n" + 
					"	/** @see X#foo() */\n" + 
					"	public void foo() {}\n" + 
					"}\n",
				"Z.java",
				"public class Z {\n" + 
					"	/** \n" + 
					"	 * @see X#x\n" + 
					"	 * @deprecated\n" + 
					"	 */\n" + 
					"	public int x;\n" + 
					"	/**\n" + 
					"	 * @see X#foo()\n" + 
					"	 * @deprecated\n" + 
					"	 */\n" + 
					"	public void foo() {}\n" + 
					"}\n"
			},
			"",
			null,
			true,
			null,
			options,
			null
		);
	}

