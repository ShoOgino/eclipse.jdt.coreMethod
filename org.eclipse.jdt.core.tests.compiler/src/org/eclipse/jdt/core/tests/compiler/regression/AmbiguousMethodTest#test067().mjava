public void test067() {
	this.runConformTest(
		new String[] {
			"A.java",
			"import java.util.*;\n" + 
			"public class A {\n" + 
			"	void foo(Collection<Object[]> c) {}\n" +
			"	public static void main(String[] args) {\n" +
			"		new B().foo(new ArrayList<Object>());\n" +
			"	}\n" +
			"}\n" + 
			"class B extends A {\n" + 
			"	void foo(ArrayList<Object> a) {System.out.print(1);}\n" + 
			"}"
		},
		"1"
	);
}

