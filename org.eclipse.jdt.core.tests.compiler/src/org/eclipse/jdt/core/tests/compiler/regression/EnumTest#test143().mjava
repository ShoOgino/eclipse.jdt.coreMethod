public void test143() {
	this.runNegativeTest(
		new String[] {
				"X.java",
				"public enum X {\n" + 
				"  A {\n" + 
				"    @Override\n" + 
				"    public String toString() {\n" + 
				"      return a();\n" + 
				"    }\n" + 
				"    public abstract String a();\n" + 
				"  }\n" + 
				"}",
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 7)\n" + 
		"	public abstract String a();\n" + 
		"	                       ^^^\n" + 
		"The abstract method a in type new X(){} can only be defined by an abstract class\n" + 
		"----------\n");
}

