public void test009() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"/**\n" + 
			" * @deprecated\n" + 
			" */\n" + 
			"public class X {\n" + 
			"}\n",
			
			"Y.java",
			"/**\n" + 
			" * @deprecated\n" + 
			" */\n" + 
			"public class Y {\n" + 
			"  Zork z;\n" +
			"  void foo() {\n" + 
			"    X x; // unexpected deprecated warning here\n" + 
			"  }\n" + 
			"}\n",
		}, 
		"----------\n" + 
		"1. ERROR in Y.java (at line 5)\n" + 
		"	Zork z;\n" + 
		"	^^^^\n" + 
		"Zork cannot be resolved to a type\n" + 
		"----------\n");
}

