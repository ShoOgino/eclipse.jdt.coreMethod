			this.runNegativeTest(
				new String[] {
					"X.java",
					"public class X {\n" + 
					"	 void foo(boolean dummy) {\n" + 
					"		 Object flag = null;\n" + 
					"		 for (;dummy;) {\n" +
					"		   flag = new Object();\n" +
					"		 }\n" +
					"		 flag.toString();\n" + 
					"	 }\n" + 
					"}\n"},
				""
			);
		}
	}

