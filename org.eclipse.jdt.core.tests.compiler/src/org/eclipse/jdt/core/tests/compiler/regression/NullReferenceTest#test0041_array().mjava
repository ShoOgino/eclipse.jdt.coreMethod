	// null analysis -- array
	public void test0041_array() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 public static void main(String args[]) {\n" + 
				"		 args[0] = null;\n" +
				"    if (args[0] == null) {};\n" + 
				     // quiet: we don't keep track of all array elements
				"	 }\n" + 
				"}\n"},
			""
		);
	}

