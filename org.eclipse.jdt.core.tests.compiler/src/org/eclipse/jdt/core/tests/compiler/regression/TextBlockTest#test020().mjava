	/*
	 * positive - using variable assigned with text block as a method argument
	 */
	public void test020() {
		runConformTest(
				new String[] {
						"X.java",
						"public class X {\n" +
						"	public static void main(String[] args) {\n" +
						"		String html = \"\"\"\n" +
						"              <html>\n" +
						"                  <body>\n" +
						"                      <p>Hello, world</p>\n" +
						"                  </body>\n" +
						"              </html>\n" +
						"                  \"\"\";\n" +
						"		System.out.println(html);\n" +
						"	}\n" +
						"}\n"
				},
				"<html>\n" +
				"    <body>\n" +
				"        <p>Hello, world</p>\n" +
				"    </body>\n" +
				"</html>",
				null,
				new String[] {"--enable-preview"});
	}

