/*
 * Completion on a qualified name reference, where the cursor is in the 
 * first type reference. 
 */
public void testQualifiedNameReferenceShrinkAll() {
	this.runTestCheckMethodParse(
		// compilationUnit:
		"class Bar {								\n" +
		"	void foo() {							\n" +
		"		int i = 0;							\n" +
		"		com.ibm.tests.Xxx o = new Y(i);		\n" +
		"	}										\n" +
		"}											\n",
		// completeBehind:
		"com",
		// expectedCompletionNodeToString:
		"<CompleteOnName:com>",
		// expectedUnitDisplayString:
		"class Bar {\n" + 
		"  Bar() {\n" + 
		"  }\n" + 
		"  void foo() {\n" +
		"    int i;\n" + 
		"    <CompleteOnName:com>;\n" +
		"  }\n" + 
		"}\n",
		// expectedCompletionIdentifier:
		"com",
		// expectedReplacedSource:
		"com",
		// test name
		"<complete on qualified name reference (shrink all)>"
	);
}

