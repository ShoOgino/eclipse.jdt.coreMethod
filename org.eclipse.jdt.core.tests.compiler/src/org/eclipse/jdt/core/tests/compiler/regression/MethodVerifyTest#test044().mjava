	// ensure AccOverriding remains when attempting to override final method 
	public void test044() {
		this.runConformTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	\n" + 
				"	final void foo() {}\n" + 
				"}\n" + 
				"class XS extends X {\n" + 
				"	@Override\n" + 
				"	void foo() {}\n" + 
				"}\n"
			},
			"----------\n" + 
			"1. ERROR in X.java (at line 7)\r\n" + 
			"	void foo() {}\r\n" + 
			"	     ^^^^^\n" + 
			"Cannot override the final method from X\n" + 
			"----------\n");
	}	

