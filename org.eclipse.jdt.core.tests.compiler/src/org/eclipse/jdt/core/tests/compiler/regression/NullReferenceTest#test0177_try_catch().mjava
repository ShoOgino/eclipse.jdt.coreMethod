	// null analysis - try/catch
	public void test0177_try_catch() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean dummy;\n" + 
				"	 Object v;\n" + 
				"	 void foo() {\n" + 
				"		 Object o = v;\n" + 
				"		 try {\n" + 
				"			 if (this.dummy) {\n" + 
				"			   o = null;\n" + 
				"			   throw new Exception();\n" + 
				"			 }\n" + 
				"		 }\n" + 
				"		 catch (Exception e) {\n" + 
				"			 o.toString();\n" +
				 	// complain: know o is null despite the lack of definite assignment
				"		 }\n" + 
				"	 }\n" + 
				"}\n"},
			"WARN"
		);
	}

