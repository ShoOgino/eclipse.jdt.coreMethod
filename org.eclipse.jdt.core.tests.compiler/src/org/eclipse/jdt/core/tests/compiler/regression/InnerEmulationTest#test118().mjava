public void test118() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"	void foo() {}\n" + 
			"	class M {\n" + 
			"		M(Object o) {}\n" + 
			"		M() {\n" + 
			"			this(new Object() {\n" + 
			"				void baz() {\n" + 
			"					foo();\n" + 
			"					bar();\n" + 
			"				}\n" + 
			"			});\n" + 
			"			new Object() {\n" + 
			"				void baz() {\n" + 
			"					foo();\n" + 
			"					bar();\n" + 
			"				}\n" + 
			"			};\n" + 
			"		}\n" + 
			"		void bar() {}\n" + 
			"		void baz() {\n" + 
			"			new Object() {\n" + 
			"				void baz() {\n" + 
			"					foo();\n" + 
			"					bar();\n" + 
			"				}\n" + 
			"			};\n" + 
			"		}\n" + 
			"	}\n" + 
			"}\n",
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 9)\n" + 
		"	bar();\n" + 
		"	^^^\n" + 
		"Cannot refer to an instance method while explicitly invoking a constructor\n" + 
		"----------\n" + 
		"2. WARNING in X.java (at line 22)\n" + 
		"	void baz() {\n" + 
		"	     ^^^^^\n" + 
		"The method baz() from the type new Object(){} is never used locally\n" + 
		"----------\n");
}

