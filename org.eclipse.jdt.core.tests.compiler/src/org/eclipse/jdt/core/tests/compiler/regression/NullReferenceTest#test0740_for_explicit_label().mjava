// null analysis - for
// https://bugs.eclipse.org/bugs/show_bug.cgi?id=176472
// variant
public void test0740_for_explicit_label() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  void foo(int i) {\n" + 
			"    Object o = null;\n" + 
			"    done: for (;;) {\n" + 
			"      switch (i) {\n" + 
			"        case 0:\n" + 
			"          o = new Object();\n" + 
			"          break;\n" + 
			"        case 1:\n" + 
			"          break done;\n" + 
			"      }\n" + 
			"    }\n" + 
			"    if (o == null) {\n" + 
			"    }\n" + 
			"  }\n" + 
			"}\n"},
		"");
} 

