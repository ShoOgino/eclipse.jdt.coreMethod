	// null analysis -- while
	public void test0124_while_unknown_attribute() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 Object o;\n" + 
				"	 void foo(boolean dummy) {\n" + 
				"		 while (dummy) {\n" +
				"		   o = null;\n" +
				"		 }\n" +
				"		 o.toString();\n" +
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

