		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" +
				"	public void count(int ... values) {}\n" +
				"}\n" +
				"class Y extends X {\n" +
				"	public void count(int[] values) {}\n" +
				"}\n",
			},			"----------\n" + 
			"1. WARNING in X.java (at line 5)\n" + 
			"	public void count(int[] values) {}\n" + 
			"	  ^^^^^\n" + 
			"The method count(int[]) is overriding a varargs method from X\n" + 
			"----------\n");
	}

