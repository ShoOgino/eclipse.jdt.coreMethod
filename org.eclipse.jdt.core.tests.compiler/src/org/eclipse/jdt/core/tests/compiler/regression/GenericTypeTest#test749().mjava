//https://bugs.eclipse.org/bugs/show_bug.cgi?id=100149
public void test749() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"import java.util.List;\n" + 
			"\n" + 
			"public class X<T extends X<?>> {\n" + 
			"	T get() { return null; }\n" + 
			"	void foo(X x) {\n" + 
			"		String s = x.get();\n" + 
			"	}\n" + 
			"}\n",
		},
		"----------\n" + 
		"1. WARNING in X.java (at line 5)\n" + 
		"	void foo(X x) {\n" + 
		"	         ^\n" + 
		"Type safety: X is a raw type. References to generic type X<T> should be parameterized\n" + 
		"----------\n" + 
		"2. ERROR in X.java (at line 6)\n" + 
		"	String s = x.get();\n" + 
		"	           ^^^^^^^\n" + 
		"Type mismatch: cannot convert from X to String\n" + 
		"----------\n");
}

