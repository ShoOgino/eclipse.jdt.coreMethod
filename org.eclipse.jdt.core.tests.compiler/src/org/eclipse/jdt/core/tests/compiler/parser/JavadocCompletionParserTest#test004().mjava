	public void test004() {
		String source = "package javadoc;\n" +
			"/**\n" +
			" * Completion with several letters:\n" +
			" * 	@deprec\n" +
			" */\n" +
			"public class Test {}\n";
		verifyCompletionInJavadoc(source, "@deprec");
		verifyCompletionOnJavadocTag("deprec".toCharArray(), new char[][] { TAG_DEPRECATED }, false);
	}

