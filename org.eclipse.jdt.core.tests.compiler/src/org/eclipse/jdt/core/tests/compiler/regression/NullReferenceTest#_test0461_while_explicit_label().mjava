// null analysis - while
// https://bugs.eclipse.org/bugs/show_bug.cgi?id=176472
// extraneous error in case of a labeled while(true) statement
public void _test0461_while_explicit_label() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  void foo(boolean b) {\n" + 
			"    Object o = null;\n" + 
			"    done: while (true) {\n" + 
			"      if (b) {\n" + 
			"        break done;\n" + 
			"      }\n" + 
			"    }\n" + 
			"    if (o == null) {\n" + 
			"    }\n" + 
			"  }\n" + 
			"}\n"},
		"");
} 

