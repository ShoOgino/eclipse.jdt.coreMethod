/*
 * Complete behind catch variable
 */
public void test19() {
	this.runTestCheckMethodParse(
		// compilationUnit:
		"public class Test {					\n" +
		"	void foo() {						\n" +
		"		try {							\n" +
		"		} catch (Exception e) {			\n" +
		"		}								\n" +
		"		e								\n" +
		"	}									\n" +
		"}										\n",
		// completeBehind:
		"\n\t\te",
		// expectedCompletionNodeToString:
		"<CompleteOnName:e>",
		// expectedUnitDisplayString:
		"public class Test {\n" + 
		"  public Test() {\n" + 
		"  }\n" + 
		"  void foo() {\n" + 
		"    <CompleteOnName:e>;\n" + 
		"  }\n" + 
		"}\n",
		// expectedCompletionIdentifier:
		"e",
		// expectedReplacedSource:
		"e",
		// test name
		"<complete behind catch variable>"
	);
}

