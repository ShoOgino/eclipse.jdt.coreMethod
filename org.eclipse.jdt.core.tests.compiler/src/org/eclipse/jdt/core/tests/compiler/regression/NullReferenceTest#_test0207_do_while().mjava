	// null analysis -- do while
	public void _test0207_do_while() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"	   boolean dummyFlag;\n" + 
				"		 do {\n" + 
				"		   o.toString();\n" + 
				      	 // complain: NPE on first iteration 
				"		   o = new Object();\n" + 
				"		 }\n" + 
				"		 while (dummyFlag);\n" + 
				"	 }\n" + 
				"}\n"},
			"WARN"  
		);
	}

