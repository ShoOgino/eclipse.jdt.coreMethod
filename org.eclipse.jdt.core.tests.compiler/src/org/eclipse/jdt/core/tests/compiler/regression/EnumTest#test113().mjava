public void test113() {
    this.runNegativeTest(
        new String[] {
            "X.java",
			"enum BugDemo {\n" +
			"	FOO() {\n" +
			"		static int bar;\n" +
			"	}\n" +
			"}\n",
        },
		"----------\n" +
		"1. ERROR in X.java (at line 3)\n" +
		"	static int bar;\n" +
		"	           ^^^\n" +
		"The field bar cannot be declared static; static fields can only be declared in static or top level types\n" +
		"----------\n");
}

