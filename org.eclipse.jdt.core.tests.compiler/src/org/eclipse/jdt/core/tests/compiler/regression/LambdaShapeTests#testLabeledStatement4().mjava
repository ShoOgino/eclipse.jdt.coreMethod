public void testLabeledStatement4() {
	this.runConformTest(
		new String[] {
			"X.java",
			"interface I {\n" +
			"	String foo();\n" +
			"}\n" +
			"interface J {\n" +
			"	void foo();\n" +
			"}\n" +
			"public class X {\n" +
			"	static void goo(I i) {\n" +
			"		System.out.println(\"I\");\n" +
			"	}\n" +
			"	static void goo(J i) {\n" +
			"		System.out.println(\"J\");\n" +
			"	}\n" +
			"	public static void main(String[] args) {\n" +
			"		goo(() -> {\n" +
			"			outerlabel: \n" +
			"			label: \n" +
			"			while (true) {\n" +
			"				while (true) {\n" +
			"					break label;\n" +
			"				}\n" +
			"			}\n" +
			"		});\n" +
			"	}\n" +
			"}\n" 
		},
		"J");
}

