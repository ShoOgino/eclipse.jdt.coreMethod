public void test1075() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X <T extends X<?>.J>{\n" + 
			"	public class J implements I<T>{}\n" + 
			"}\n",
			"I.java",
			"public interface I <T> {}\n",
			"Y.java",
			"public class Y extends X {}\n",
		},
		""
	);

	this.runConformTest(
		new String[] {
			"Y.java",
			"public class Y extends X {}",
		},
		"",
		null,
		false, // do not flush output
		null);
}

