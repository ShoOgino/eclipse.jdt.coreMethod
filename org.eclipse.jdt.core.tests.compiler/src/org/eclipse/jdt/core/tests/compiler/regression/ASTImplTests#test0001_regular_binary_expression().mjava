// AST implementation - visiting binary expressions
public void test0001_regular_binary_expression() {
	runConformTest(
		"X.java", 
		"public class X {\n" + 
		"  void foo() {\n" + 
		"    String s1 = \"s1\";\n" + 
		"    String s2 = \"s2\";\n" + 
		"    String s3 = \"s3\";\n" + 
		"    String s4 = \"s4\";\n" + 
		"    System.out.println(s1 + \"l1\" + s2 + \"l2\" +\n" +
		"      s3 + \"l3\" + s4);\n" + 
		"  }\n" + 
		"}\n",
		defaultParser,
		new ASTBinaryExpressionCollector(),
		"[v SL \"s1\"]\n" + 
		"[ev SL \"s1\"]\n" + 
		"[v SL \"s2\"]\n" + 
		"[ev SL \"s2\"]\n" + 
		"[v SL \"s3\"]\n" + 
		"[ev SL \"s3\"]\n" + 
		"[v SL \"s4\"]\n" + 
		"[ev SL \"s4\"]\n" + 
		"[v BE ((((((s1 + \"l1\") + s...) + s4)]\n" + 
		"[v BE (((((s1 + \"l1\") + s2...+ \"l3\")]\n" + 
		"[v BE ((((s1 + \"l1\") + s2)...) + s3)]\n" + 
		"[v BE (((s1 + \"l1\") + s2) + \"l2\")]\n" + 
		"[v BE ((s1 + \"l1\") + s2)]\n" + 
		"[v BE (s1 + \"l1\")]\n" + 
		"[v SNR s1]\n" + 
		"[ev SNR s1]\n" + 
		"[v SL \"l1\"]\n" + 
		"[ev SL \"l1\"]\n" + 
		"[ev BE (s1 + \"l1\")]\n" + 
		"[v SNR s2]\n" + 
		"[ev SNR s2]\n" + 
		"[ev BE ((s1 + \"l1\") + s2)]\n" + 
		"[v SL \"l2\"]\n" + 
		"[ev SL \"l2\"]\n" + 
		"[ev BE (((s1 + \"l1\") + s2) + \"l2\")]\n" + 
		"[v SNR s3]\n" + 
		"[ev SNR s3]\n" + 
		"[ev BE ((((s1 + \"l1\") + s2)...) + s3)]\n" + 
		"[v SL \"l3\"]\n" + 
		"[ev SL \"l3\"]\n" + 
		"[ev BE (((((s1 + \"l1\") + s2...+ \"l3\")]\n" + 
		"[v SNR s4]\n" + 
		"[ev SNR s4]\n" + 
		"[ev BE ((((((s1 + \"l1\") + s...) + s4)]\n");
}

