public void test106() {
	this.runConformTest(
			new String[] {
				"pack/X.java",
				"package pack;\n" +
				"import static pack.Color.*;\n" +
				"public class X {\n" +
				"    public static void main(String[] args) {\n" +
				"        Color c = BLACK;\n" +
				"        switch(c) {\n" +
				"        }\n" +
				"		 System.out.print(\"SUCCESS\");\n" +
				"    }\n" +
				"}",
				"pack/Color.java",
				"package pack;\n" + 
				"enum Color {WHITE, BLACK}"
			},
			"SUCCESS"
		);
	
	this.runConformTest(
		new String[] {
			"pack/Color.java",
			"package pack;\n" + 
			"enum Color {BLACK, WHITE}"
		},
		"",
		null,
		false,
		null
	);
	
	this.executeClass(
		"pack/X.java",
		"SUCCESS",
		null,
		false,
		null,
		null,
		null);	
}

