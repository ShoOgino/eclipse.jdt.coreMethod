public void test021() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"interface I {\n" + 
			"	String foo(int x) throws Exception;\n" + 
			"}\n" + 
			"public class X {\n" + 
			"	void goo(I i) {}\n" + 
			"	void test() {\n" + 
			"		goo((x) -> {\n" + 
			"			int i = 100;\n" + 
			"			outer: while(x > 0) {\n" + 
			"				inner: while(i > 0) {\n" + 
			"				if(--i > 50) {\n" + 
			"					return \"\";\n" + 
			"				}\n" + 
			"				if(i > 90) {\n" + 
			"					break outer;\n" + 
			"				}\n" + 
			"				return \"\";\n" + 
			"				}\n" + 
			"			}});\n" + 
			"	}\n" + 
			"}\n"
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 7)\n" + 
		"	goo((x) -> {\n" + 
		"	    ^^^^^^\n" + 
		"This method must return a result of type String\n" + 
		"----------\n" + 
		"2. WARNING in X.java (at line 10)\n" + 
		"	inner: while(i > 0) {\n" + 
		"	^^^^^\n" + 
		"The label inner is never explicitly referenced\n" + 
		"----------\n");
}

