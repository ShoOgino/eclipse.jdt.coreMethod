public void test61() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" +
			"	java.sql.Date d = new java.util.Date();\n" + 
			"}\n",
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 2)\n" + 
		"	java.sql.Date d = new java.util.Date();\n" + 
		"	                  ^^^^^^^^^^^^^^^^^^^^\n" + 
		"Type mismatch: cannot convert from java.util.Date to java.sql.Date\n" + 
		"----------\n");
}

