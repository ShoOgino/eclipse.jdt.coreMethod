	// null analysis -- for
	public void test0227_for() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean bar() {\n" + 
				"		 return true;\n" + 
				"	 }\n" + 
				"	 void foo(Object o) {\n" + 
				"		 for (;bar() && o == null; o.toString()) {\n" + 
				"		   o = new Object();\n" +
				"		 }\n" + 
				"	 }\n" + 
				"}\n"},
			""
		);
	}

