// [1.5][compiler] ClassCastException on illegal code fragment
public void _test917() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  class X1<T> { \n" + 
			"      public static Class clazz = T.getClass();\n" + 
			"  }\n" + 
			"  class X2<T> { }\n" + 
			"  class X3<T> extends X2<T.clazz> { }\n" + 
			"}\n",
		},
		"ERR");
}

