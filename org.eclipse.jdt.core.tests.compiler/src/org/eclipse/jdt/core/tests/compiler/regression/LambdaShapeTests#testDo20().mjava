public void testDo20() {
	this.runConformTest(
		new String[] {
			"X.java",
			"interface I {\n" +
			"	String foo();\n" +
			"}\n" +
			"interface J {\n" +
			"	void foo();\n" +
			"}\n" +
			"public class X {\n" +
			"	static void goo(I i) {\n" +
			"		System.out.println(\"I\");\n" +
			"	}\n" +
			"	static void goo(J i) {\n" +
			"		System.out.println(\"J\");\n" +
			"	}\n" +
			"	public static void main(String[] args) {\n" +
			"		goo(() -> {\n" +
			"			do {\n" +
			"				switch (args.length){\n" +
			"				default:\n" +
			"					continue;\n" +
			"				}\n" +
			"			} while (false);	\n" +
			"		});\n" +
			"	}\n" +
			"}\n"
		},
		"J");
}

