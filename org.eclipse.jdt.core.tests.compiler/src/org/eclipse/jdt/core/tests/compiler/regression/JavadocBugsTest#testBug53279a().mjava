	public void testBug53279a() {
		runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" +
					"	/**\n" +
					"	 * Unterminated inline tags\n" +
					"	 *  {@link Object\n" +
					"	 * @return int\n" +
					"	 */\n" +
					"	int foo() {return 0;}\n" +
					"}\n"
			},
			"----------\n" +
				"1. ERROR in X.java (at line 4)\n" +
				"	*  {@link Object\n" +
				"	   ^^^^^^^^^^^^^\n" +
				"Javadoc: Missing closing brace for inline tag\n" +
				"----------\n",
				JavacTestOptions.Excuse.EclipseWarningConfiguredAsError
		);
	}

