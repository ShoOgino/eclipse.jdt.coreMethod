			"    	Foo<? extends C> h = A::baz;\n" + 
			"    	h.get(0,  null);\n" + 
			"    	Foo<? extends C> h2 = D::new;\n" + 
			"    	h2.get(0,  null);\n" + 
			"    	Goo g = A::bar;\n" + 
			"    	g.m(null);\n" + 
