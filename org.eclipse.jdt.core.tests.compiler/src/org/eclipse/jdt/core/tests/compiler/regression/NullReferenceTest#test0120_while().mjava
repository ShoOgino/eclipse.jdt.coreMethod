	// null analysis -- while
	public void test0120_while() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean dummy;\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"		 while (dummy || (o = new Object()).equals(o)) {\n" +
				"		   o.toString();\n" +
				"		   o = new Object();\n" +
				"		 }\n" +
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

