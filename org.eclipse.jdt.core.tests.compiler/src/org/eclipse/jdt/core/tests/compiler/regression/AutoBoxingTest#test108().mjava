public void test108() {
	this.runConformTest(
		new String[] {
			"X.java",
			"class XSuper<T> {\n" + 
			"	T value;\n" + 
			"}\n" + 
			"public class X extends XSuper<Integer>{\n" + 
			"	public static void a(X x) {\n" + 
			"		x.value--;\n" + 
			"		--x.value;\n" + 
			"		x.value -= 1;\n" + 
			"		x.value = x.value - 1;\n" + 
			"		System.out.println(x.value);\n" + 
			"	}\n" + 
			"\n" + 
			"	public static void main(final String[] args) {\n" + 
			"		X x = new X();\n" +
			"		x.value = 5;\n" +
			"		a(x);\n" + 
			"	}\n" + 
			"}\n",
		},
		"1");
}

