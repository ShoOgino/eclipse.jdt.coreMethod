void runConformTestWithLibs(String[] testFiles, Map customOptions, String expectedCompilerLog, String expectedOutput) {
	runConformTest(
			false, /* flush output directory */
			testFiles,
			this.LIBS,
			customOptions,
			expectedCompilerLog,
			expectedOutput,
			"",/* expected error */
		    JavacTestOptions.Excuse.EclipseWarningConfiguredAsError);
}

