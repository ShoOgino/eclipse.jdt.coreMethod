public void test052() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public enum X {\n" + 
			"	A\n" + 
			"	;\n" + 
			"	\n" + 
			"	public abstract void foo();\n" + 
			"}\n"
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 2)\n" + 
		"	A\n" + 
		"	^\n" + 
		"The enum constant A must define the abstract method foo()\n" + 
		"----------\n"
	);
}		

