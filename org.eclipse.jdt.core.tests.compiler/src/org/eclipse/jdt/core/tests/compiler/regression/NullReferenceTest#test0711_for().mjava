// null analysis -- for
public void test0711_for() {
	if (COMPLIANCE_1_5.compareTo(this.complianceLevel) <= 0) {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"  void foo() {\n" + 
				"    Object t[] = null;\n" + 
				"    for (Object o : t) {/* */}\n" +
				      // complain: NPE
				"  }\n" + 
				"}\n"},
			"----------\n" + 
			"1. ERROR in X.java (at line 4)\n" + 
			"	for (Object o : t) {/* */}\n" + 
			"	                ^\n" + 
			"Null pointer access: The variable t can only be null at this location\n" +  
			"----------\n");
	}
}

