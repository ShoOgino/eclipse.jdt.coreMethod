// null analysis - shortcut boolean expression and correlation
// https://bugs.eclipse.org/bugs/show_bug.cgi?id=195774
public void _test0087_shortcut_boolean_expression() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  public static int foo(Integer i, Integer j) {\n" + 
			"    if (i == null && j == null) {\n" + 
			"      return 1;\n" + 
			"    }\n" + 
			"    if (j == null) {\n" + 
			"      return i.intValue();\n" + 
			"    }\n" + 
			"    if (i == null) {\n" + 
			"      return j.intValue();\n" +
			"    }\n" + 
			"    return 0;\n" + 
			"  }\n" + 
			"}"},
		"");
}

