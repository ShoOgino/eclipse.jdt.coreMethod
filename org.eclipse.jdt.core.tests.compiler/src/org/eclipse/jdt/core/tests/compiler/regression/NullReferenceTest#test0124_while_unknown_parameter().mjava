	// null analysis -- while
	public void test0124_while_unknown_parameter() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 boolean dummy;\n" + 
				"	 void foo(Object o) {\n" + 
				"		 while (dummy) {\n" +
				"		   o = null;\n" +
				"		 }\n" +
				"		 o.toString();\n" +
				"	 }\n" + 
				"}\n"},
			""  
		);
	}

