	/**
	 * Locate the given file.
	 * @return Returns null if file couldn't be found.
	 */
	private File locate(String fileName) { 
		if (fileName != null) {
			fileName= fileName.replace('.', '/')+".class";
			File path= null;
			for (int i= 0; i < pathItems.length; i++) {
				path= new File(pathItems[i], fileName);
				if (path.exists())
					return path;
			}
		}
		return null;
	}

