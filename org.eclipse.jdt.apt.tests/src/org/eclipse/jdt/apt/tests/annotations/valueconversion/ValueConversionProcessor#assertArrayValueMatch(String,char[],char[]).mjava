	private void assertArrayValueMatch(
			final String name,
			final char[] actual,
			final char[] expected){
		
		int i=0;
		final Messager msgr = _env.getMessager(); 
		for( char a : actual ){
			if( a != expected[i] ){
				msgr.printError("value mismatch for member " + name + 
						" expected " + expected[i] + " but got " + a);
			}
			i++;
		}
	}

