	public void process() {
		Filer f = _env.getFiler();
		try {
			PrintWriter writer = f.createTextFile(
					Filer.Location.CLASS_TREE, 
					"", 
					new File(FILE_NAME), 
					null);
			writer.print(TEXT);
			writer.close();
			
		}
		catch (IOException ioe) {
			throw new RuntimeException("Could not generate text file",ioe);
		}
	}

