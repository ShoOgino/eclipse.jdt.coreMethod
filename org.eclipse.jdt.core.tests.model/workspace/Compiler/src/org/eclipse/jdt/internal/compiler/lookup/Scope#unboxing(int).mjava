	/*
	 * Unboxing primitive
	 */
	public int unboxing(int id) {
		switch (id) {
			case T_JavaLangInteger :
				return T_int;
			case T_JavaLangByte :
				return T_byte;
			case T_JavaLangShort :
				return T_short;
			case T_JavaLangCharacter :
				return T_char;
			case T_JavaLangLong :
				return T_long;
			case T_JavaLangFloat :
				return T_float;
			case T_JavaLangDouble :
				return T_double;
			case T_JavaLangBoolean :
				return T_boolean;
			case T_JavaLangVoid :
				return T_void;
		}
		return id;
	}

