	/*
	 * Ensures that a generic constructor binding can be created using its key in batch creation.
	 */
	public void test061() throws CoreException {
		assertRequestedBindingFound(
			new String[] {
				"/P/p1/X.java",
				"package p1;\n" +
				"public class X {\n" + 
				"    /*start*/<T> X() {\n" + 
				"    }/*end*/\n" + 
				"}",
			}, 
			"Lp1/X;.<T:Ljava/lang/Object;>()V^0"
		);
	}

