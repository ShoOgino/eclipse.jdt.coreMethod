	/*
	 * Ensures that an annotation binding can be retrieved using its key.
	 */
	public void test087() throws CoreException {
		assertRequestedBindingFound(
			new String[] {
				"/P/p1/X.java",
				"package p1;\n" +
				"public class X {\n" +
				"  /*start*/@MyAnnot/*end*/\n" +
				"  void foo() {\n" +
				"  }\n" +
				"}",
				"/P/p1/MyAnnot.java",
				"package p1;\n" +
				"public @interface MyAnnot {\n" +
				"}",
			}, 
			"Lp1/X;.foo()V@Lp1/MyAnnot;");
	}

