	/*
	 * Ensure that a parameterized type binding can be created using its key.
	 */
	public void test028() throws CoreException {
		assertBindingCreated(
			new String[] {
				"/P/p1/X.java",
				"package p1;\n" +
				"public class X<T> {\n" +
				"  X<String> field;\n" +
				"}",
			},
			"p1/X<java.lang/String,>");
	}

