	this.formatterPrefs.indent_body_declarations_compare_to_enum_declaration_header = false;
	String source =
		"public enum Test {\n" + 
		"AAA, BBB;\n" + 
		"\n" + 
		"Test() {\n" + 
		"}\n" + 
		"}";
