	this.formatterPrefs.preserve_existing_line_breaks = true;
	String source = 
		"public class X {\n" + 
		"\n" + 
		"	/**\n" + 
		"	 * @see java.lang.String\n" + 
		"	 * \n" + 
		"	 * Formatter should keep empty line above\n" + 
		"	 */\n" + 
		"	void foo() {\n" + 
		"	}\n" + 
		"}\n";
	formatSource(source,
		"public class X {\n" + 
		"\n" + 
		"	/**\n" + 
		"	 * @see java.lang.String\n" + 
		"	 * \n" + 
		"	 *      Formatter should keep empty line above\n" + 
