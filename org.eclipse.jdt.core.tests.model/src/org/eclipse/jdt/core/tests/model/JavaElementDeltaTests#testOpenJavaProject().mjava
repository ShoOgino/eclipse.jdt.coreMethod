/*
 * Open a java project.
 */
public void testOpenJavaProject() throws CoreException {
	try {
		this.createJavaProject("P", new String[] {""}, "");
		IProject project = this.getProject("P");
		project.close(null);
		this.startDeltas();
		project.open(null);
		assertEquals(
			"Unexpected delta", 
			"P[+]: {}", 
			this.getDeltas());
	} finally {
		this.stopDeltas();
		this.deleteProject("P");
	}
}

