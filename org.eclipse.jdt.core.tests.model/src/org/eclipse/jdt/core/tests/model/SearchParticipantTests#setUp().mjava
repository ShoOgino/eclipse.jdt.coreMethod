	protected void setUp() throws Exception {
		super.setUp();
		if (this.restart) {
			simulateRestart();
			this.restart = false;
		}
		if (this.project == null) {
			this.project = createJavaProject("P");
		}
		this.deleteProject = true;
		if (JobManager.VERBOSE) {
			System.out.println("--------------------------------------------------------------------------------");
			System.out.println("Run test "+getName()+"...");
		}
	}

