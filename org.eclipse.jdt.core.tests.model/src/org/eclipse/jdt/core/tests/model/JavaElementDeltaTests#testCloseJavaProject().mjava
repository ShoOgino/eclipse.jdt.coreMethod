/*
 * Close a java project.
 */
public void testCloseJavaProject() throws CoreException {
	try {
		createJavaProject("P", new String[] {""}, "");
		IProject project = getProject("P");
		startDeltas();
		project.close(null);
		assertDeltas(
			"Unexpected delta", 
			"P[*]: {CLOSED}\n" + 
			"ResourceDelta(/P)"
		);
	} finally {
		stopDeltas();
		deleteProject("P");
	}
}

