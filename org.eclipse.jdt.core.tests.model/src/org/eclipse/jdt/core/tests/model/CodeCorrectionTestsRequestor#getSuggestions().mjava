	public String getSuggestions(){
		Suggestion[] suggestions = getSortedSuggestions();
		
		StringBuffer result = new StringBuffer();
		for (int i = 0; i < suggestions.length; i++) {
			if(i != 0)
				result.append('\n');
				
			result.append(suggestions[i].text);
		}
		return result.toString();
	}

