/*
 * Rename a non-java project.
 */
public void testRenameProject() throws CoreException {
	try {
		this.createProject("P");
		this.startDeltas();
		this.renameProject("P", "P1");
		assertEquals(
			"Unexpected delta", 
			"", 
			this.getDeltas());
	} finally {
		this.stopDeltas();
		this.deleteProject("P");
		this.deleteProject("P1");
	}
}

