	public void tearDownSuite() throws Exception {
		if (TEST_SUITES == null) {
			deleteProject("JavaSearch");
			deleteProject("JavaSearch15");
			deleteProject("JavaSearchBugs");
		} else {
			TEST_SUITES.remove(getClass());
			if (TEST_SUITES.size() == 0) {
				deleteProject("JavaSearch");
				deleteProject("JavaSearch15");
				deleteProject("JavaSearchBugs");
			}
		}
	
		super.tearDownSuite();
	}

