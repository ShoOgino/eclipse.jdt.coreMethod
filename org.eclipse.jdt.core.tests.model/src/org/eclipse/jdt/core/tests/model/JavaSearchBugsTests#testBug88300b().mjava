	workingCopies[0] = getWorkingCopy("/JavaSearchBugs/src/b88300/SubClass.java",
		"package b88300;\n" + 
		"public class SubClass extends SuperClass {\n" + 
		"	public void aMethod(Object x) {\n" + 
