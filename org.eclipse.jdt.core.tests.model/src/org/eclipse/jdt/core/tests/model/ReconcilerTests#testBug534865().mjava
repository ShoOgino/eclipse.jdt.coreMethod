					"@Documented\n" + 
					"@Retention(RetentionPolicy.CLASS)\n" + 
					"@Target({ TYPE_USE })\n" + 
					"public @interface Nullable {\n" + 
					"	// marker annotation with no members\n" + 
					"}\n"
		);
		String source = 
				"package org.eclipse.jdt.annotation;\n" +
				"import static java.lang.annotation.ElementType.TYPE_USE;\n" + 
