/*
 * Add the java nature to an existing project.
 */
public void testAddJavaNature() throws CoreException {
	try {
		this.createProject("P");
		this.startDeltas();
		this.addJavaNature("P");
		assertDeltas(
			"Unexpected delta", 
			"P[+]: {}"
		);
	} finally {
		this.stopDeltas();
		this.deleteProject("P");
	}
}

