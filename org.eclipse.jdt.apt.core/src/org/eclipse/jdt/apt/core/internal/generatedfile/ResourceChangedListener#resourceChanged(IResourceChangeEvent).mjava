	public void resourceChanged(IResourceChangeEvent event) 
	{
		if ( event.getType() == IResourceChangeEvent.POST_CHANGE )
		{
			try
			{
				event.getDelta().accept( new Visitor() );
			}
			catch ( CoreException ce )
			{
				// TODO:  handle exception here.
				ce.printStackTrace();
			}
		}
	}

