	private Set<IFile> cleanupAllGeneratedFilesFrom(
			IFile[] files,
			Set<IFile> deletedFiles){
		if( deletedFiles == null )
			deletedFiles = new HashSet<IFile>();
		
		if( files != null ){
			for(IFile f : files ){				
				cleanupAllGeneratedFilesForParent( f, null, deletedFiles );
			}
		}
		return deletedFiles;
	}

