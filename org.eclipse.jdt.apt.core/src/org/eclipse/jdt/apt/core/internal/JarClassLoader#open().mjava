	public synchronized void open() {
		_openCounter++;
		if (_openCounter == 1) {
			// Create all jar files
			_jars = new ArrayList<JarFile>(_files.size());
			for (File f : _files) {
				try {
					JarFile jar = new JarFile(f);
					_jars.add(jar);
				}
				catch (IOException ioe) {
					AptPlugin.log(ioe, "Unable to create JarFile for file: " + f); //$NON-NLS-1$
				}
			}
		}
	}

