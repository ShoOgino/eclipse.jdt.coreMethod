	public ICompilationUnit getICompilationUnitForFile(final IFile file){
		if( file == null ) 
    		return null;
    	else if( file.equals(_file) )
    		return _unit;
    	else if( _batchMode ){
    		for( int i=0, len=_files.length; i<len; i++ ){
        		if( file.equals(_files[i]) )
        			return _units[i];
        	}
    	}
    	return null;
	}

