	/**
	 * scan the source code to see if there are any annotation tokens
	 */
	public static boolean hasAnnotationInstance( IFile f )
	{
		try
		{
			char[] source = ProcessorEnvImpl.getFileContents( f );
			return hasAnnotationInstance( source );
		}
		catch( Exception ioe )
		{
			return false;
		}
	}

