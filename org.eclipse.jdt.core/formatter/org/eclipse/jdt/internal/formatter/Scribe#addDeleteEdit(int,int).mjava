	private final void addDeleteEdit(int start, int end) {
		if (end < start) return;
		if (this.textRegionStart < start && end <= this.textRegionEnd) {
			if (this.edits.length == this.editsIndex) {
				// resize
				resize();
			}
			addOptimizedReplaceEdit(start, end - start + 1, ""); //$NON-NLS-1$
		}
	}

