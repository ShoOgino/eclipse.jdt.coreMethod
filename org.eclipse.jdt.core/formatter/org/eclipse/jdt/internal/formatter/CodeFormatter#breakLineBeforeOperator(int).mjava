	/**
	 * Returns true if a lineSeparator has to be inserted before <code>operator</code>
	 * false otherwise.
	 */
	private static boolean breakLineBeforeOperator(int operator) {
		switch (operator) {
			case TokenNameCOMMA :
			case TokenNameSEMICOLON :
			case TokenNameEQUAL :
				return false;
			default :
				return true;
		}
	}

