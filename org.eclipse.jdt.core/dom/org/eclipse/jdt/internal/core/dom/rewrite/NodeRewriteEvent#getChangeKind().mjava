	/* (non-Javadoc)
	 * @see org.eclipse.jdt.internal.corext.dom.RewriteEvent#getChangeKind()
	 */
	public int getChangeKind() {
		if (fOriginalValue == fNewValue) {
			return UNCHANGED;
		}
		if (fOriginalValue == null) {
			return INSERTED;
		}
		if (fNewValue == null) {
			return REMOVED;
		}
		if (fOriginalValue.equals(fNewValue)) {
			return UNCHANGED;
		}
		return REPLACED;
	}

