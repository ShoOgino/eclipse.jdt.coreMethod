		public String toString() {
			StringBuffer buf= new StringBuffer();
			if (first != last) {
				buf.append("range ");  //$NON-NLS-1$
			}
			if (isMove) {
				buf.append("move source: "); //$NON-NLS-1$
			} else {
				buf.append("copy source: "); //$NON-NLS-1$
			}
			buf.append(first);
			if (first != last) {
				buf.append(" - "); //$NON-NLS-1$
				buf.append(last);
			}
			return buf.toString();
		}

