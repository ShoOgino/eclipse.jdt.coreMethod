	/*
	 * @see ASTVisitor#visit(ReturnStatement)
	 */
	public boolean visit(ReturnStatement node) {
		buffer.append("return");//$NON-NLS-1$
		if (node.getExpression() != null) {
			buffer.append(" ");//$NON-NLS-1$
			node.getExpression().accept(this);
		}
		buffer.append(";");//$NON-NLS-1$
		return false;
	}

