	boolean equalSubtrees(Object other) {
		if (!(other instanceof SuperFieldAccess)) {
			return false;
		}
		SuperFieldAccess o = (SuperFieldAccess) other;
		return 
			(ASTNode.equalNodes(getName(), o.getName())
			&& ASTNode.equalNodes(getQualifier(), o.getQualifier()));
	}

