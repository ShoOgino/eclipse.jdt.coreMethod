	boolean equalSubtrees(Object other) {
		if (!(other instanceof ExpressionStatement)) {
			return false;
		}
		ExpressionStatement o = (ExpressionStatement) other;
		return ASTNode.equalNodes(getExpression(), o.getExpression());
	}

