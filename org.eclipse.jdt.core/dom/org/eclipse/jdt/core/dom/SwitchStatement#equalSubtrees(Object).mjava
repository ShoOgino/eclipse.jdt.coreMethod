	boolean equalSubtrees(Object other) {
		if (!(other instanceof SwitchStatement)) {
			return false;
		}
		SwitchStatement o = (SwitchStatement) other;
		return 
			(ASTNode.equalNodes(getExpression(), o.getExpression())
			&& ASTNode.equalLists(statements(), o.statements()));
	}

