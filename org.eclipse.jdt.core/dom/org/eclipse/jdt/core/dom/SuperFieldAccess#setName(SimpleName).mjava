	/**
	 * Sets the name of the field accessed in this "super" field access 
	 * expression.
	 * 
	 * @param fieldName the field name
	 * @exception IllegalArgumentException if:
	 * <ul>
	 * <li>the node belongs to a different AST</li>
	 * <li>the node already has a parent</li>
	 * </ul>
	 */ 
	public void setName(SimpleName fieldName) {
		if (fieldName == null) {
			throw new IllegalArgumentException();
		}
		preReplaceChild(this.fieldName, fieldName, NAME_PROPERTY);
		this.fieldName = fieldName;
		postReplaceChild(this.fieldName, fieldName, NAME_PROPERTY);
	}

