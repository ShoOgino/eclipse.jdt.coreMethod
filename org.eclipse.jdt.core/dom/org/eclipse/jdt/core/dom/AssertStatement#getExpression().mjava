	/**
	 * Returns the first expression of this assert statement.
	 * 
	 * @return the expression node
	 */ 
	public Expression getExpression() {
		if (expression == null) {
			preLazyInit();
			this.expression = new SimpleName(this.ast);
			postLazyInit(this.expression, EXPRESSION_PROPERTY);
		}
		return expression;
	}

