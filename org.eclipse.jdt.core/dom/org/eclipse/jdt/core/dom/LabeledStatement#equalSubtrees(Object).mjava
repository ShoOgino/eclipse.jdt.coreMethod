	boolean equalSubtrees(Object other) {
		if (!(other instanceof LabeledStatement)) {
			return false;
		}
		LabeledStatement o = (LabeledStatement) other;
		return 
			(ASTNode.equalNodes(getLabel(), o.getLabel())
			&& ASTNode.equalNodes(getBody(), o.getBody()));
	}

