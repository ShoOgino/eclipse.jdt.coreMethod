	boolean equalSubtrees(Object other) {
		if (!(other instanceof FieldAccess)) {
			return false;
		}
		FieldAccess o = (FieldAccess) other;
		return 
			(ASTNode.equalNodes(getExpression(), o.getExpression())
			&& ASTNode.equalNodes(getName(), o.getName()));
	}

