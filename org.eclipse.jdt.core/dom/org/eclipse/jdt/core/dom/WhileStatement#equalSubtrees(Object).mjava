	boolean equalSubtrees(Object other) {
		if (!(other instanceof WhileStatement)) {
			return false;
		}
		WhileStatement o = (WhileStatement) other;
		return 
			(ASTNode.equalNodes(getExpression(), o.getExpression())
			&& ASTNode.equalNodes(getBody(), o.getBody()));
	}

