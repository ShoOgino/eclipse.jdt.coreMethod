	/**
	 * @since 3.0
	 * @deprecated Replaced by getType(), which returns a Type
	 * TODO (jeem) - Remove before M9
	 */ 
	public Name getName() {
		if (this.typeName == null) {
			preLazyInit();
			this.typeName = new SimpleName(this.ast);
			postLazyInit(this.typeName, NAME_PROPERTY);
		}
		return this.typeName;
	}

