	/**
	 * @exception UnsupportedOperationException if this operation is used below JLS10
	 * @since 3.14
	 */
	@Override
	public boolean isVar() {
		unsupportedBelow10();
		if (this.typeName == null) getName();
		return this.typeName instanceof SimpleName && ((SimpleName) this.typeName).isVar();
	}

