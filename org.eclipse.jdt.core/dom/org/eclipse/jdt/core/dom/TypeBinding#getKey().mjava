	/*
	 * @see IBinding#getKey()
	 */
	public String getKey() {
		if (this.key == null) {
			if (false) {
				this.key = new String(this.binding.computeUniqueKey());
				return this.key;
			}
			StringBuffer buffer = new StringBuffer();
			appendKey(buffer);
			this.key = buffer.toString();
		}
		return this.key;
	}

