	boolean equalSubtrees(Object other) {
		if (!(other instanceof CastExpression)) {
			return false;
		}
		CastExpression o = (CastExpression) other;
		return 
		 (ASTNode.equalNodes(getType(), o.getType())
		 && ASTNode.equalNodes(getExpression(), o.getExpression()));
	}

