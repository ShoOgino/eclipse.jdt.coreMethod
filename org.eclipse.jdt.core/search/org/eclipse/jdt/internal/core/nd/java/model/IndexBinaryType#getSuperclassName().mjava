	@Override
	public char[] getSuperclassName() {
		try (IReader rl = this.typeRef.lock()) {
			NdType type = this.typeRef.get();
			if (type != null) {
				NdTypeSignature superclass = type.getSuperclass();
				if (superclass != null) {
					return superclass.getRawType().getBinaryName();
				}
				return null;
			} else {
				return new char[0];
			}
		}
	}

