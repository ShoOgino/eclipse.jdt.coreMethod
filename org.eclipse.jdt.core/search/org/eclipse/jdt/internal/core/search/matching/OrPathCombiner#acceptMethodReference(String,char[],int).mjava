	public void acceptMethodReference(
		String resourcePath,
		char[] methodName,
		int parameterCount) {
		if (!this.acceptedAnswers.containsKey(resourcePath)) {
			this.acceptedAnswers.put(resourcePath, resourcePath);
			this.targetRequestor.acceptMethodReference(
				resourcePath,
				methodName,
				parameterCount);
		}
	}

