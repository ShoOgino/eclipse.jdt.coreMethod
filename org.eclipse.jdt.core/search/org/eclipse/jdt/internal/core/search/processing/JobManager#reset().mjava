	/**
	 * Flush current state
	 */
	public void reset() {

		if (thread != null) {
			discardJobs(null); // discard all jobs
		} else {
			/* initiate background processing */
			thread = new Thread(this, this.processName());
			thread.setDaemon(true);
			thread.setPriority(Thread.NORM_PRIORITY-1);
			thread.start();
		}
	}

