	public JavaSearchMatch(
			IResource resource,
			IJavaElement element,
			String documentPath,
			int accuracy,  
			SearchParticipant participant,
			int sourceStart, 
			int sourceEnd, 
			int sourceLineNumber) {
		super(
			element.getElementName(), 
			documentPath, 
			accuracy, 
			participant, 
			sourceStart, 
			sourceEnd, 
			sourceLineNumber, 
			element.toString());
		this.resource = resource;
		this.element = element;
	}

