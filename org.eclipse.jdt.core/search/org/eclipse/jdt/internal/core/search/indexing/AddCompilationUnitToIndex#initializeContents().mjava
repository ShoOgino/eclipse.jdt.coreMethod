	public void initializeContents() {
		if (!resource.isLocal(IResource.DEPTH_ZERO)) {
			return;
		} else {
			try {
				IPath location = resource.getLocation();
				if (location != null) {
					this.contents =
						org.eclipse.jdt.internal.compiler.util.Util.getFileCharContent(
							location.toFile(), null);
				}
			} catch (IOException e) {
			}
		}
	}

