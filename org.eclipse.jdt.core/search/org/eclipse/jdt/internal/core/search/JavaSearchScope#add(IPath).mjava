	/**
	 * Adds the given path to this search scope.
	 */
	private void add(IPath path) {
		if (this.paths.length == this.pathsCount) {
			System.arraycopy(
				this.paths,
				0,
				this.paths = new IPath[this.pathsCount * 2],
				0,
				this.pathsCount);
		}
		this.paths[this.pathsCount++] = path;
	}

