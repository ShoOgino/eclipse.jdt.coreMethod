public void initializeContents() {
	if (!resource.isLocal(IResource.DEPTH_ZERO)) {
		return;
	} else {
		try {
			IPath location = resource.getLocation();
			if (location != null){;
				this.contents = Util.getFileByteContent(resource.getLocation().toFile());
			}
		} catch (IOException e) {
		}
	}
}

