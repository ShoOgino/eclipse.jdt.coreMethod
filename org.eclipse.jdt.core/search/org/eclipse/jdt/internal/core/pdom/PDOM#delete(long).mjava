	private void delete(long address) {
		if (address == 0) {
			return;
		}
		short nodeType = PDOMNode.NODE_TYPE.get(this, address);
	
		// Look up the type
		ITypeFactory<? extends PDOMNode> factory1 = getTypeFactory(nodeType);
	
		// Call its destructor
		factory1.destruct(this, address);
	
		// Free up its memory
		getDB().free(address);
	}

