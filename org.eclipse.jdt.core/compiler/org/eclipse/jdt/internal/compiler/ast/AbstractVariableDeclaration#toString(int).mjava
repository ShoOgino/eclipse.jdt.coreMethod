public String toString(int tab) {
	/*slow code*/

	String s = tabString(tab);
	if (modifiers != AccDefault) {
		s += modifiersString(modifiers);
	}
	s += type.toString(0) + " " + new String(name());
	if (initialization != null)
		s += " = " + initialization.toStringExpression(tab);
	return s;
}

