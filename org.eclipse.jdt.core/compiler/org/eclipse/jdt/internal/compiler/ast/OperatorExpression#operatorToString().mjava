public String operatorToString() {
	switch ((bits & OperatorMASK) >> OperatorSHIFT) {
		case EQUAL_EQUAL :
			return "==";
		case LESS_EQUAL :
			return "<=";
		case GREATER_EQUAL :
			return ">=";
		case NOT_EQUAL :
			return "!=";
		case LEFT_SHIFT :
			return "<<";
		case RIGHT_SHIFT :
			return ">>";
		case UNSIGNED_RIGHT_SHIFT :
			return ">>>";
		case OR_OR :
			return "||";
		case AND_AND :
			return "&&";
		case PLUS :
			return "+";
		case MINUS :
			return "-";
		case NOT :
			return "!";
		case REMAINDER :
			return "%";
		case XOR :
			return "^";
		case AND :
			return "&";
		case MULTIPLY :
			return "*";
		case OR :
			return "|";
		case TWIDDLE :
			return "~";
		case DIVIDE :
			return "/";
		case GREATER :
			return ">";
		case LESS :
			return "<";
		case QUESTIONCOLON :
			return "?:";
		case EQUAL :
			return "=";
	};
	return "unknown operator";
}

