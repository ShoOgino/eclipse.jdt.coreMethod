public String toStringExpression() {
	/* slow code */

	String s = "{" ;
	if (expressions != null)
	{ 	int j = 20 ; 
		for (int i = 0 ; i < expressions.length ; i++)
		{	s = s + expressions[i].toStringExpression() + "," ;
			j -- ;
			if (j == 0)
			{	s = s + "\n                "; j = 20;}}};
	s = s + "}";
	return s;}

