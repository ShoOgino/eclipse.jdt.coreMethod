public String toStringExpression() {

	String s = "{" ; //$NON-NLS-1$
	if (expressions != null)
	{ 	int j = 20 ; 
		for (int i = 0 ; i < expressions.length ; i++)
		{	s = s + expressions[i].toStringExpression() + "," ; //$NON-NLS-1$
			j -- ;
			if (j == 0)
			{	s = s + "\n                "; j = 20;}}}; //$NON-NLS-1$
	s = s + "}"; //$NON-NLS-1$
	return s;}

