public String toStringExpression() {
	/* slow code */

	String s = "{"/*nonNLS*/ ;
	if (expressions != null)
	{ 	int j = 20 ; 
		for (int i = 0 ; i < expressions.length ; i++)
		{	s = s + expressions[i].toStringExpression() + ","/*nonNLS*/ ;
			j -- ;
			if (j == 0)
			{	s = s + "\n                "/*nonNLS*/; j = 20;}}};
	s = s + "}"/*nonNLS*/;
	return s;}

