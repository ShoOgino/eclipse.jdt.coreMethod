public String toString(int tab){
	/* slow code */
	
	String s = ""; //$NON-NLS-1$
	if (modifiers != AccDefault){
		s += modifiersString(modifiers);
	}
	if (type == null){
		s += "<no type> "; //$NON-NLS-1$
	} else {
		s += type.toString(tab) + " "; //$NON-NLS-1$
	}
	s += new String(name);
	return s;
}

