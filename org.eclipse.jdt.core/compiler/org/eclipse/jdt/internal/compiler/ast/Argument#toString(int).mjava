public String toString(int tab){
	/* slow code */
	
	String s = ""/*nonNLS*/;
	if (modifiers != AccDefault){
		s += modifiersString(modifiers);
	}
	if (type == null){
		s += "<no type> "/*nonNLS*/;
	} else {
		s += type.toString(tab) + " "/*nonNLS*/;
	}
	s += new String(name);
	return s;
}

