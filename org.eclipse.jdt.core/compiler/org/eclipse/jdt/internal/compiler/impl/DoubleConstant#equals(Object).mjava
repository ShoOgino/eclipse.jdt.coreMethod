	public boolean equals(Object obj) {
		if (this == obj) {
			return true;
		}
		if (obj == null) {
			return false;
		}
		if (getClass() != obj.getClass()) {
			return false;
		}
		DoubleConstant other = (DoubleConstant) obj;
		return Double.doubleToLongBits(this.value) == Double.doubleToLongBits(other.value);
	}

