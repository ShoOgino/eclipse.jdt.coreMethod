public ReferenceBinding superclass() {
	if ((this.tagBits & HasUnresolvedSuperclass) == 0)
		return this.superclass;

	this.superclass = resolveType(this.superclass, this.environment, true);
	this.tagBits ^= HasUnresolvedSuperclass;
	return this.superclass;
}
