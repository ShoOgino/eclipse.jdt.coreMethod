	boolean hasOnlyRawBounds() {
		if (this.superclass != null && !this.superclass.isRawType())
			return false;

		if (this.superInterfaces != null)
			for (int i = 0, l = this.superInterfaces.length; i < l; i++)
		   		if (!this.superInterfaces[i].isRawType())
		   			return false;

		return true;
	}

