// NOT Public API
public final Binding getImport(char[][] compoundName, boolean onDemand, boolean isStaticImport) {
	if (onDemand)
		return findOnDemandImport(compoundName, isStaticImport);
	return findSingleImport(compoundName, isStaticImport);
}
