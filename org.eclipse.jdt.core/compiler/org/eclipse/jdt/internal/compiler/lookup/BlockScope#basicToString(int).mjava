String basicToString(int tab) {
	String newLine = "\n";
	for (int i = tab; --i >= 0;)
		newLine += "\t";

	String s = newLine + "--- Block Scope ---";
	newLine += "\t";
	s += newLine + "locals:";
	for (int i = 0; i < localIndex; i++)
		s += newLine + "\t" + locals[i].toString();
	s += newLine + "startIndex = " + startIndex;
	return s;
}

