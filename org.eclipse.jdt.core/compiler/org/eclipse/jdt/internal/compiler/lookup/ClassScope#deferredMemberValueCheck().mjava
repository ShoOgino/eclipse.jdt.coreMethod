	private void deferredMemberValueCheck() {
		if (this.deferredBoundChecks != null) {
			Iterator iterator = this.deferredBoundChecks.iterator();
			while (iterator.hasNext()) {
				Object check = iterator.next();
				if (check instanceof Runnable) {
					((Runnable)check).run();
					iterator.remove();
				}
			}
		}
	}

