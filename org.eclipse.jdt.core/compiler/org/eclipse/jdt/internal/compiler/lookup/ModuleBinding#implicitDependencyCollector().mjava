	// All implicit dependencies offered by this module
	public Supplier<Collection<ModuleBinding>> implicitDependencyCollector() {
		return () -> getRequiredModules(true)
			.collect(HashSet::new,
				(set, mod) -> {
					if (set.add(mod)) {
						collectImplicitDependencies(set, mod);
					}
				},
			HashSet::addAll);
	}

