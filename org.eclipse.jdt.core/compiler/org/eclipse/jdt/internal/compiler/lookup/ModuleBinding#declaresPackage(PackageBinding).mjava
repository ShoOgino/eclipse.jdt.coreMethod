	public boolean declaresPackage(PackageBinding p) {
		PackageBinding pkg = this.declaredPackages.get(p.readableName());
		if (pkg == null) {
			pkg = getDeclaredPackage(p.compoundName);
			if (pkg == p) {
				this.declaredPackages.put(p.readableName(), p);
				return true;
			}
		}
		return pkg == p;
	}

