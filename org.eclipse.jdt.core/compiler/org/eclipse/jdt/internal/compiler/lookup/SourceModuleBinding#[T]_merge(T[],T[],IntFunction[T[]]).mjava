	private <T> T[] merge(T[] one, T[] two, IntFunction<T[]> supplier) {
		if (one.length == 0) {
			if (two.length > 0)
				return two;
			return one;
		}
		int len1 = one.length;
		int len2 = two.length;
		T[] result = supplier.apply(len1+len2);
		System.arraycopy(one, 0, result, 0, len1);
		System.arraycopy(two, 0, result, len1, len2);
		return result;
	}

