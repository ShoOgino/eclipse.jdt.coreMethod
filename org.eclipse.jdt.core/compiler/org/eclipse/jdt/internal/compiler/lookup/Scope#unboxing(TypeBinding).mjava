	/*
	 * Unboxing primitive
	 */
	public TypeBinding unboxing(TypeBinding type) {
		switch (type.id) {
			case T_JavaLangInteger :
				return IntBinding;
			case T_JavaLangByte :
				return ByteBinding;
			case T_JavaLangShort :
				return ShortBinding;		
			case T_JavaLangCharacter :
				return CharBinding;				
			case T_JavaLangLong :
				return LongBinding;
			case T_JavaLangFloat :
				return FloatBinding;
			case T_JavaLangDouble :
				return DoubleBinding;
			case T_JavaLangBoolean :
				return BooleanBinding;
			case T_JavaLangVoid :
				return VoidBinding;
		}
		return type;
	}		

