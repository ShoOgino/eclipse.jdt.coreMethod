	public boolean hasConflict(ModuleBinding clientModule) {
		int visibleCount = 0;
		for (PackageBinding incarnation : this.incarnations) {
			if (incarnation.hasCompilationUnit()) {
				if (clientModule.canAccess(incarnation)) 
					if (++visibleCount > 1)
						return true;
			}
		}
		return false;
	}

