public boolean isReferencing(TypeVariableBinding variable) {
	if (this == variable) {
		return true;
	}
	if (this.firstBound.isReferencing(variable)) {
		return true;
	}
	for (int i = 0, l = this.superInterfaces.length ; i < l ; i++) {
		if (this.superInterfaces[i].isReferencing(variable)) {
			return true;
		}
	}
	return false;
}

