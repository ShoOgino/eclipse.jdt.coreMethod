	public void invalidOperator(
		CompoundAssignment assign,
		TypeBinding leftType,
		TypeBinding rightType) {
		this.handle(
			InvalidOperator,
			new String[] {
				assign.operatorToString(),
				new String(leftType.readableName())
					+ ", "
					+ new String(rightType.readableName())},
			assign.sourceStart,
			assign.sourceEnd);
	}

