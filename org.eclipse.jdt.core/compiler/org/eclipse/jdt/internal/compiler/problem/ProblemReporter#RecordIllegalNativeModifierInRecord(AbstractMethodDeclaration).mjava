public void RecordIllegalNativeModifierInRecord(AbstractMethodDeclaration method) {
	if (!this.options.enablePreviewFeatures)
		return;
	this.handle(
		IProblem.RecordIllegalNativeModifierInRecord,
		new String[] { new String(method.selector)},
		new String[] { new String(method.selector)},
		method.sourceStart,
		method.sourceEnd);
}

