	public static JimageFileSystem getJimageSystem(File image) {
		JimageFileSystem system = null;
		if (images == null) {
			images = new HashMap<>();
			images.put(image, system = new JimageFileSystem(image));
			return system;
		}
		system = images.get(image);
		if (system == null) {
			images.put(image, system = new JimageFileSystem(image));
		}
		return system;
	}

