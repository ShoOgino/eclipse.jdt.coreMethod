	public int removeKey(Object key) {

		int index = key.hashCode() % valueTable.length;
		Object currentKey;
		while ((currentKey = keyTable[index]) != null) {
			if (currentKey.equals(key)) {
				int value = valueTable[index];
				elementSize--;
				keyTable[index] = null;
				valueTable[index] = -1;
				rehash();
				return value;
			}
			index = (index + 1) % keyTable.length;
		}
		return -1;
	}

