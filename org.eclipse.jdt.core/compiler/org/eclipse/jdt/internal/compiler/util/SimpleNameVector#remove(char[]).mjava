	public char[] remove(char[] element) {
		// assumes only one occurrence of the element exists
		for (int i = size; --i >= 0;)
			if (element == elements[i]) {
				// shift the remaining elements down one spot
				System.arraycopy(elements, i + 1, elements, i, --size - i);
				elements[size] = null;
				return element;
			}
		return null;
	}

