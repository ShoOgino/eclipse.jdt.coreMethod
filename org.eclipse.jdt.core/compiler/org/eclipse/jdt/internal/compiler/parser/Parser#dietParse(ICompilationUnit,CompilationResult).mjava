	public CompilationUnitDeclaration dietParse(
		ICompilationUnit sourceUnit,
		CompilationResult compilationResult) {

		CompilationUnitDeclaration parsedUnit;
		boolean old = diet;
		try {
			diet = true;
			parsedUnit = parse(sourceUnit, compilationResult);
		} finally {
			diet = old;
		}
		return parsedUnit;
	}

