	protected void consumeImportDeclarationsopt() {
		// ImportDeclarationsopt ::= ImportDeclarations
		int length;
		if ((length = astLengthStack[astLengthPtr--]) != 0) {
			astPtr -= length;
			System.arraycopy(
				astStack,
				astPtr + 1,
				compilationUnit.imports = new ImportReference[length],
				0,
				length);
		}
	}

