public void lastore() {
	if (DEBUG) System.out.println(this.position + "\t\tlastore"); //$NON-NLS-1$
	this.countLabels = 0;
	this.stackDepth -= 4;
	if (this.classFileOffset >= this.bCodeStream.length) {
		resizeByteArray();
	}
	this.position++;
	this.bCodeStream[this.classFileOffset++] = Opcodes.OPC_lastore;
}

