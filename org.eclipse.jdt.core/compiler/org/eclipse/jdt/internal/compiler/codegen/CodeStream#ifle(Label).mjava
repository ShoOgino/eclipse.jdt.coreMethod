final public void ifle(Label lbl) {
	countLabels = 0;
	stackDepth--;
	if (this.wideMode) {
		generateWideConditionalBranch(OPC_ifle, lbl);
	} else {
		try {
			position++;
			bCodeStream[classFileOffset++] = OPC_ifle;
		} catch (IndexOutOfBoundsException e) {
			resizeByteArray(OPC_ifle);
		}
		lbl.branch();
	}
}

