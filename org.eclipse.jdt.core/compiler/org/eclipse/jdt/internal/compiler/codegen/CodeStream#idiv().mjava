public void idiv() {
	if (DEBUG) System.out.println(this.position + "\t\tidiv"); //$NON-NLS-1$
	this.countLabels = 0;
	this.stackDepth--;
	if (this.classFileOffset >= this.bCodeStream.length) {
		resizeByteArray();
	}
	this.position++;
	this.bCodeStream[this.classFileOffset++] = Opcodes.OPC_idiv;
}

