final public void if_icmple(Label lbl) {
	countLabels = 0;
	stackDepth -= 2;
	if (this.wideMode) {
		generateWideConditionalBranch(OPC_if_icmple, lbl);
	} else {	
		try {
			position++;
			bCodeStream[classFileOffset++] = OPC_if_icmple;
		} catch (IndexOutOfBoundsException e) {
			resizeByteArray(OPC_if_icmple);
		}
		lbl.branch();
	}
}

