final public void ifne(Label lbl) {
	countLabels = 0;
	stackDepth--;
	if (this.wideMode) {
		generateWideConditionalBranch(OPC_ifne, lbl);
	} else {
		try {
			position++;
			bCodeStream[classFileOffset++] = OPC_ifne;
		} catch (IndexOutOfBoundsException e) {
			resizeByteArray(OPC_ifne);
		}
		lbl.branch();
	}
}

