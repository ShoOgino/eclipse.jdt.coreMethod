public void lstore_1() {
	if (DEBUG) System.out.println(this.position + "\t\tlstore_1"); //$NON-NLS-1$
	this.countLabels = 0;
	this.stackDepth -= 2;
	if (this.maxLocals < 3) {
		this.maxLocals = 3;
	}
	if (this.classFileOffset >= this.bCodeStream.length) {
		resizeByteArray();
	}
	this.position++;
	this.bCodeStream[this.classFileOffset++] = Opcodes.OPC_lstore_1;
}

