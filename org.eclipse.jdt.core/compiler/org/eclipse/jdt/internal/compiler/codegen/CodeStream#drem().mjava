public void drem() {
	if (DEBUG) System.out.println(this.position + "\t\tdrem"); //$NON-NLS-1$
	this.countLabels = 0;
	this.stackDepth -= 2;
	if (this.classFileOffset >= this.bCodeStream.length) {
		resizeByteArray();
	}
	this.position++;
	this.bCodeStream[this.classFileOffset++] = Opcodes.OPC_drem;
}

