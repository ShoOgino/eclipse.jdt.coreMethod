public void lstore_3() {
	if (DEBUG) System.out.println(this.position + "\t\tlstore_3"); //$NON-NLS-1$
	this.countLabels = 0;
	this.stackDepth -= 2;
	if (this.maxLocals < 5) {
		this.maxLocals = 5;
	}
	if (this.classFileOffset >= this.bCodeStream.length) {
		resizeByteArray();
	}
	this.position++;
	this.bCodeStream[this.classFileOffset++] = Opcodes.OPC_lstore_3;
}

