public void generateReturnBytecode(Expression expression) {
	
	if (expression == null) {
		this.return_();
	} else {
		final int implicitConversion = expression.implicitConversion;
		if ((implicitConversion & BOXING) != 0) {
			this.areturn();
			return;
		}
		int runtimeType = (implicitConversion & IMPLICIT_CONVERSION_MASK) >> 4;
		switch (runtimeType) {
			case T_boolean :
			case T_int :
				this.ireturn();
				break;
			case T_float :
				this.freturn();
				break;
			case T_long :
				this.lreturn();
				break;
			case T_double :
				this.dreturn();
				break;
			default :
				this.areturn();
		}
	}
}

