public void lstore_0() {
	if (DEBUG) System.out.println(this.position + "\t\tlstore_0"); //$NON-NLS-1$
	this.countLabels = 0;
	this.stackDepth -= 2;
	if (this.maxLocals < 2) {
		this.maxLocals = 2;
	}
	if (this.classFileOffset >= this.bCodeStream.length) {
		resizeByteArray();
	}
	this.position++;
	this.bCodeStream[this.classFileOffset++] = Opcodes.OPC_lstore_0;
}

