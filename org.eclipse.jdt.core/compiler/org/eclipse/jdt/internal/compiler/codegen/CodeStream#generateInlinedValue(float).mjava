	public void generateInlinedValue(float inlinedValue) {
		if (inlinedValue == 0.0f) {
			if (Float.floatToIntBits(inlinedValue) != 0)
				this.ldc(inlinedValue);
			else
				this.fconst_0();
			return;
		}
		if (inlinedValue == 1.0f) {
			this.fconst_1();
			return;
		}
		if (inlinedValue == 2.0f) {
			this.fconst_2();
			return;
		}
		this.ldc(inlinedValue);
	}

