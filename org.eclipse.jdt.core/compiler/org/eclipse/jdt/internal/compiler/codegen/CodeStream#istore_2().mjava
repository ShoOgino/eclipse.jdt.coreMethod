public void istore_2() {
	if (DEBUG) System.out.println(this.position + "\t\tistore_2"); //$NON-NLS-1$
	this.countLabels = 0;
	this.stackDepth--;
	if (this.maxLocals <= 2) {
		this.maxLocals = 3;
	}
	if (this.classFileOffset >= this.bCodeStream.length) {
		resizeByteArray();
	}
	this.position++;
	this.bCodeStream[this.classFileOffset++] = Opcodes.OPC_istore_2;
}

