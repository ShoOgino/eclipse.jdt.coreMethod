final public void ifeq(Label lbl) {
	countLabels = 0;
	stackDepth--;
	if (this.wideMode) {
		generateWideConditionalBranch(OPC_ifeq, lbl);
	} else {
		try {
			position++;
			bCodeStream[classFileOffset++] = OPC_ifeq;
		} catch (IndexOutOfBoundsException e) {
			resizeByteArray(OPC_ifeq);
		}
		lbl.branch();
	}
}

