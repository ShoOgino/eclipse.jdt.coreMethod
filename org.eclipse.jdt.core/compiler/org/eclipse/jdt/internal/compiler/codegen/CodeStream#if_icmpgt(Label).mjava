final public void if_icmpgt(Label lbl) {
	countLabels = 0;
	stackDepth -= 2;
	if (this.wideMode) {
		generateWideRevertedConditionalBranch(OPC_if_icmple, lbl);
	} else {	
		try {
			position++;
			bCodeStream[classFileOffset++] = OPC_if_icmpgt;
		} catch (IndexOutOfBoundsException e) {
			resizeByteArray(OPC_if_icmpgt);
		}
		lbl.branch();
	}
}

