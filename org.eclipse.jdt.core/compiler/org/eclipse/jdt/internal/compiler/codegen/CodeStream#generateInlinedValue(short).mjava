	public void generateInlinedValue(short inlinedValue) {
		switch (inlinedValue) {
			case -1 :
				this.iconst_m1();
				break;
			case 0 :
				this.iconst_0();
				break;
			case 1 :
				this.iconst_1();
				break;
			case 2 :
				this.iconst_2();
				break;
			case 3 :
				this.iconst_3();
				break;
			case 4 :
				this.iconst_4();
				break;
			case 5 :
				this.iconst_5();
				break;
			default :
				if ((-128 <= inlinedValue) && (inlinedValue <= 127)) {
					this.bipush((byte) inlinedValue);
					return;
				}
				this.sipush(inlinedValue);
		}
	}

