final public void iflt(Label lbl) {
	countLabels = 0;
	stackDepth--;
	if (this.wideMode) {
		generateWideConditionalBranch(OPC_iflt, lbl);
	} else {
		try {
			position++;
			bCodeStream[classFileOffset++] = OPC_iflt;
		} catch (IndexOutOfBoundsException e) {
			resizeByteArray(OPC_iflt);
		}
		lbl.branch();
	}
}

