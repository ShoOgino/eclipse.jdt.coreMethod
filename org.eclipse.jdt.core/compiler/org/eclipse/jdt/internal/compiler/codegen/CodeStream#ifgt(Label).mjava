final public void ifgt(Label lbl) {
	countLabels = 0;
	stackDepth--;
	if (this.wideMode) {
		generateWideConditionalBranch(OPC_ifgt, lbl);
	} else {
		try {
			position++;
			bCodeStream[classFileOffset++] = OPC_ifgt;
		} catch (IndexOutOfBoundsException e) {
			resizeByteArray(OPC_ifgt);
		}
		lbl.branch();
	}
}

