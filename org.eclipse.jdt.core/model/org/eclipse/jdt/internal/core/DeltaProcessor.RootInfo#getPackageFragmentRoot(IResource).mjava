		public IPackageFragmentRoot getPackageFragmentRoot(IResource resource) {
			if (this.root == null) {
				if (resource != null) {
					this.root = this.project.getPackageFragmentRoot(resource);
				} else {
					this.root = getPackageFragmentRoot();
				}
			}
			if (this.root != null) 
				this.cache = this.root;
			return this.root;
		}

