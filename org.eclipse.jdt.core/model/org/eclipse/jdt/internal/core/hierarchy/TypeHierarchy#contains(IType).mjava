/**
 * @see ITypeHierarchy
 */
public boolean contains(IType type) {
	// classes
	if (fClassToSuperclass.get(type) != null) {
		return true;
	}

	// root classes
	if (fRootClasses.contains(type)) return true;

	// interfaces
	if (fInterfaces.contains(type)) return true;
	
	return false;
}

