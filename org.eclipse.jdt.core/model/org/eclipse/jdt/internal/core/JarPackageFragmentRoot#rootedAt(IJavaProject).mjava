/*
 * @see JavaElement#rootedAt(IJavaProject)
 */
public IJavaElement rootedAt(IJavaProject project) {
	if (fResource == null) {
		return
			new JarPackageFragmentRoot(
				fJarPath.toString(),
				project);
	} else {
		return
			new JarPackageFragmentRoot(
				fResource,
				project);
	}
}

