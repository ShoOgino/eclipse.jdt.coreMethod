	/**
	 * Flush ZipFiles cache.
	 */
	public void flushZipFiles() {
		if (this.zipFiles == null) return;
		Iterator iterator = this.zipFiles.values().iterator();
		while (iterator.hasNext()) {
			try {
				((ZipFile)iterator.next()).close();
			} catch (IOException e) {
			}
		}
		this.zipFiles = null;
	}

