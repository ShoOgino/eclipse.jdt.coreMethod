	/**
	 * Java Models are equal if their workspaces are equal
	 *
	 * @see Object#equals
	 */
	public boolean equals(Object o) {
		if (this == o)
			return true;
		if (o instanceof JavaModel) {
			JavaModel other = (JavaModel) o;
			return this.workspace.equals(other.workspace);
		}
		return false;
	}

