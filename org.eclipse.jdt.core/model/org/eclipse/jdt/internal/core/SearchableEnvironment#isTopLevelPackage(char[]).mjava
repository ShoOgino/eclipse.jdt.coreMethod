	/**
	 * @see SearchableBuilderEnvironment
	 */
	public boolean isTopLevelPackage(char[] packageName) {
		if (packageName == null)
			return false;
		boolean result =
			!isQualified(packageName)
				&& this.nameLookup.findPackageFragments(new String(packageName), false) != null;
		return result;

	}

