/**
 * @see IWorkingCopy
 */
public void destroy() {
	if (--this.useCount > 0) {
		if (SHARED_WC_VERBOSE) {
			System.out.println("Decrementing use count of shared working copy " + this.toDebugString());
		}
		return;
	}
	try {
		close();
		
		// if original element is not on classpath flush it from the cache 
		IJavaElement originalElement = this.getOriginalElement();
		if (!this.getParent().exists()) {
			((CompilationUnit)originalElement).close();
		}
		
		// remove working copy from the cache
		JavaModelManager manager = JavaModelManager.getJavaModelManager();
		if (manager.sharedWorkingCopies.remove(originalElement) != null) {
			if (SHARED_WC_VERBOSE) {
				System.out.println("Destroying shared working copy " + this.toDebugString());
			}

			// report removed java delta
			JavaElementDelta delta = new JavaElementDelta(this.getJavaModel());
			delta.removed(this);
			manager.registerJavaModelDelta(delta);
			manager.fire();
		}
		
	} catch (JavaModelException e) {
		// do nothing
	}
}

