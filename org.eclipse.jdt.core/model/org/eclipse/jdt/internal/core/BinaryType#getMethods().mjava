	/**
	 * @see IType
	 */
	public IMethod[] getMethods() throws JavaModelException {
		Vector v = getChildrenOfType(METHOD);
		int size;
		if ((size = v.size()) == 0) {
			return NO_METHODS;
		} else {
			IMethod[] array = new IMethod[size];
			v.copyInto(array);
			return array;
		}
	}

