	/**
	 * @private Debugging purposes
	 */
	protected void toStringInfo(int tab, StringBuffer buffer, Object info) {
		if (getElementName().length() == 0) {
			buffer.append("[default]");
		} else {
			buffer.append(getElementName());
		}
		if (info == null) {
			buffer.append(" (not open)");
		} else {
			if (tab > 0) {
				buffer.append(" (...)");
			}
		}
	}

