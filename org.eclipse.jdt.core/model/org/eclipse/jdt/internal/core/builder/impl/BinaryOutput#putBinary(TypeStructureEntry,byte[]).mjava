/**
 * Stores the binary produced by compiling a type.
 */
public void putBinary(TypeStructureEntry tsEntry, byte[] binary) {
	/* store crc in type structure entry */
	int crc;
	
	if (COMPUTE_CRC){
		crc = crc32(binary);
		tsEntry.setCRC32(crc);
	} else {
		crc = 0;
	}
	
	/* store binary */
	basicPutBinary(tsEntry, binary, crc);
}

