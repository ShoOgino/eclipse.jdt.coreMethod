public void done() {
	updateProgress(1.0f);
	subTask(Util.bind("build.done")); //$NON-NLS-1$
	if (fProgress != null) {
		fProgress.done();
	}
	if (fBuildMonitor != null) {
		fBuildMonitor.endBuild(fIsBatch ? Util.bind("build.endBatch") : Util.bind("build.endIncremental")); //$NON-NLS-2$ //$NON-NLS-1$
	}
	this.previousSubtask = null;
}

