public void done() {
	updateProgress(1.0f);
	subTask(Util.bind("build.done"/*nonNLS*/));
	if (fProgress != null) {
		fProgress.done();
	}
	if (fBuildMonitor != null) {
		fBuildMonitor.endBuild(fIsBatch ? Util.bind("build.endBatch"/*nonNLS*/) : Util.bind("build.endIncremental"/*nonNLS*/));
	}
	this.previousSubtask = null;
}

