/**
 * Creates a problem detail.
 */
public ProblemDetailImpl(String msg, int id, int severity, SourceEntry sourceEntry, int startPos, int endPos, int lineNumber) {
	fMessage = msg;
	fID = id;
	fSeverity = severity;
	fSourceEntry = sourceEntry;
	fStartPos = startPos;
	fEndPos = endPos;
	fLineNumber = lineNumber;
}

