	/**
	 * Returns a string representation of the package.  For debugging purposes
	 * only (NON-NLS).
	 */
	public String toString() {
		String result = "package "; //$NON-NLS-1$
		if (isUnnamed()) 
			result += "{unnamed, id=" + getName() + "}"; //$NON-NLS-2$ //$NON-NLS-1$
		else
			result += getName();
		return result;
	}

