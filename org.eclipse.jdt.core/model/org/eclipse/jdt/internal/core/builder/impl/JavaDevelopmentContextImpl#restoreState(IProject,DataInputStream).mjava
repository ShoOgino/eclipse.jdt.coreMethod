/**
 * @see IDevelopmentContext.
 */
public IState restoreState(IProject project, DataInputStream in) throws IOException {
	try {
		return new StateSnap().read(this, project, in);
	}
	catch (RuntimeException e) {
		e.printStackTrace();
		throw e;
	}
	catch (Error e) {
		e.printStackTrace();
		throw e;
	}
}

