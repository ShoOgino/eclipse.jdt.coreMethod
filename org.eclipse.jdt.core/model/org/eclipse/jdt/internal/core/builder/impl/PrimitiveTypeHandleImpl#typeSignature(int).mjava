	/**
	 * Convert a base type code to the VM naming format
	 */
	protected static String typeSignature(int typeCode) {
		switch (typeCode) {
			case TC_VOID :
				return "V";
			case TC_BOOLEAN :
				return "Z";
			case TC_BYTE :
				return "B";
			case TC_CHAR :
				return "C";
			case TC_SHORT :
				return "S";
			case TC_INT :
				return "I";
			case TC_LONG :
				return "J";
			case TC_FLOAT :
				return "F";
			case TC_DOUBLE :
				return "D";
			default :
				return "unknown";
		}
	}

