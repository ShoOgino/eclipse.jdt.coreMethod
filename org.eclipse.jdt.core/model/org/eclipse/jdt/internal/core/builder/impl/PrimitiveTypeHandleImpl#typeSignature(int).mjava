/**
 * Convert a base type code to the VM naming format
 */
protected static String typeSignature(int typeCode) {
	switch (typeCode) {
		case TC_VOID :
			return "V"/*nonNLS*/;
		case TC_BOOLEAN :
			return "Z"/*nonNLS*/;
		case TC_BYTE :
			return "B"/*nonNLS*/;
		case TC_CHAR :
			return "C"/*nonNLS*/;
		case TC_SHORT :
			return "S"/*nonNLS*/;
		case TC_INT :
			return "I"/*nonNLS*/;
		case TC_LONG :
			return "J"/*nonNLS*/;
		case TC_FLOAT :
			return "F"/*nonNLS*/;
		case TC_DOUBLE :
			return "D"/*nonNLS*/;
		default :
			return "unknown"/*nonNLS*/;
	}
}

