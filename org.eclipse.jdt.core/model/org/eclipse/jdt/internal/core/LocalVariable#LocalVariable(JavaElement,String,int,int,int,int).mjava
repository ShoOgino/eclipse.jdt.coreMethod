	public LocalVariable(
			JavaElement parent, 
			String name, 
			int declarationSourceStart, 
			int declarationEnd,
			int nameStart, 
			int nameEnd) {
		
		super(parent, name);
		this.declarationSourceStart = declarationSourceStart;
		this.declarationEnd = declarationEnd;
		this.nameStart = nameStart;
		this.nameEnd = nameEnd;
	}

