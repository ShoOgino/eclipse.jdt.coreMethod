protected void openParent(IProgressMonitor pm) throws JavaModelException {
	if (FIX_BUG25184) {
		super.openParent(pm);
	} else {
		try {
			super.openParent(pm);
		} catch(JavaModelException e){
			// allow parent to not exist for compilation units defined outside classpath
			if (!e.isDoesNotExist()){ 
				throw e;
			}
		}
	}
}

