	/**
	 * @see ISourceElementRequestor
	 */
	public void enterField(
		int declarationStart,
		int modifiers,
		char[] type,
		char[] name,
		int nameSourceStart,
		int nameSourceEnd) {
		if (fNextElement.getElementType() == IJavaElement.FIELD) {
			if (isIdentical((IField) fNextElement, name)) {
				next();
				return;
			}
		}
		throw new FailedReconciliationException();
	}

