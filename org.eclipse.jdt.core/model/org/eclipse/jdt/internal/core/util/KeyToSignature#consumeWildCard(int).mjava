	public void consumeWildCard(int wildCardKind) {
		switch (wildCardKind) {
			case Wildcard.UNBOUND:
				this.signature.append('*');
				break;
			case Wildcard.EXTENDS:
				this.signature.append('+');
				break;
			case Wildcard.SUPER:
				this.signature.append('-');
				break;
			default:
				// malformed
				return;
		}
	}

