	/**
	 * @see IBytecodeVisitor#_newarray(int)
	 */
	public void _newarray(int pc, int atype) {
		writeTabs();
		buffer
			.append(pc)
			.append('\t')
			.append(OpcodeStringValues.BYTECODE_NAMES[IOpcodeMnemonics.NEWARRAY])
			.append(' ')
			.append('#')
			.append(atype)
			.append(' ');
		appendGetArrayType(atype);
		writeNewLine();
	}

