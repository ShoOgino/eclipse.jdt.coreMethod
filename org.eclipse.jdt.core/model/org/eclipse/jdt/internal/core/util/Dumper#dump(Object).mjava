	/**
	 * Dumps the given value.
	 */
	public void dump(Object val) {
		tabIfNeeded();
		if (val instanceof IDumpable) {
			IDumpable dumpable = (IDumpable) val;
			fWriter.println(classNameFor(val) + " {");
			int originalLevel = fTabLevel;
			++fTabLevel;
			try {
				dumpable.dump(this);
			} catch (Throwable t) {
				fWriter.println("*ERR*");
			}
			fTabLevel = originalLevel;
			tabIfNeeded();
			fWriter.println("}");
		} else {
			fWriter.println(val);
		}
	}

