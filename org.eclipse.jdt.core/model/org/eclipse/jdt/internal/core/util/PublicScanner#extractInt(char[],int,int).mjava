	private int extractInt(char[] array, int start, int end) {
		int value = 0;
		for (int i = start; i < end; i++) {
			final char currentChar = array[i];
			int digit = 0;
			switch(currentChar) {
				case '0' :
					digit = 0;
					break;
				case '1' :
					digit = 1;
					break;
				case '2' :
					digit = 2;
					break;
				case '3' :
					digit = 3;
					break;
				case '4' :
					digit = 4;
					break;
				case '5' :
					digit = 5;
					break;
				case '6' :
					digit = 6;
					break;
				case '7' :
					digit = 7;
					break;
				case '8' :
					digit = 8;
					break;
				case '9' :
					digit = 9;
					break;
				default :
					throw new NumberFormatException();
			}
			value *= 10;
			if (digit < 0) throw new NumberFormatException();
			value += digit;
		}
		return value;
	}

