	/**
	 * @see IBytecodeVisitor#_iload_2(int)
	 */
	public void _iload_2(int pc) {
		dumpPcNumber(pc);
		buffer.append(Util.bind("classformat.load", //$NON-NLS-1$
			new String[] {
				OpcodeStringValues.BYTECODE_NAMES[IOpcodeMnemonics.ILOAD_2],
				getLocalVariableName(pc, 2)
			}
		));
		writeNewLine();
	}

