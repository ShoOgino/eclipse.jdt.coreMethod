	public void consumeWildCard(int kind, int rank) {
		switch (kind) {
			case Wildcard.UNBOUND:
				this.arguments.add(new StringBuffer("*")); //$NON-NLS-1$
				break;
			case Wildcard.EXTENDS:
				((StringBuffer) this.arguments.get(this.arguments.size()-1)).insert(0, '+');
				break;
			case Wildcard.SUPER:
				((StringBuffer) this.arguments.get(this.arguments.size()-1)).insert(0, '-');
				break;
		}
	}

