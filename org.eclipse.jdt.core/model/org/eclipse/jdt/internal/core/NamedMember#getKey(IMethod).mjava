	protected String getKey(IMethod method) {
		StringBuffer key = new StringBuffer();
		
		// declaring class 
		String declaringKey = ((IType) method.getParent()).getKey();
		key.append(declaringKey);
		
		// selector
		key.append('.');
		String selector = method.getElementName();
		key.append(selector);
		
		// parameters
		key.append('(');
		String[] parameters = method.getParameterTypes();
		for (int i = 0, length = parameters.length; i < length; i++)
			key.append(parameters[i].replace('.', '/'));
		key.append(')');
		
		// return type
		key.append('V'); // cannot get the return type without opening the element, default to void
		
		return key.toString();
	}

