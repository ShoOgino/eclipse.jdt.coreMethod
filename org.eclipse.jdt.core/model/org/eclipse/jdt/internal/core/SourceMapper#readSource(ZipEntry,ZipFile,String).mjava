	private char[] readSource(ZipEntry entry, ZipFile zip, String charSet) {
		try {
			byte[] bytes = Util.getZipEntryByteContent(entry, zip);
			if (bytes != null) {
				return Util.bytesToChar(bytes, charSet == null ? this.encoding : charSet);
			}
		} catch (IOException e) {
			// ignore
		}
		return null;
	}

