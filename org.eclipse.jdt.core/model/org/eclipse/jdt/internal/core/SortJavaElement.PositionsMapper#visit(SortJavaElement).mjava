		void visit(SortJavaElement node) {
			for (int i = 0, max = positionsToMap.length; i < max; i++) {
				int nextPosition = positionsToMap[i];
				if (nextPosition != -1
					&& nextPosition >= node.sourceStart 
					&& nextPosition <= node.sourceEnd) {
						node.recordPosition(nextPosition);
						positionsToMap[i] = -1;
					}
			}
		}

