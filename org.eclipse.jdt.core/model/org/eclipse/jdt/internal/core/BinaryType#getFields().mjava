@Override
public IField[] getFields() throws JavaModelException {
	if (!isRecord()) {
		ArrayList list = getChildrenOfType(FIELD);
		if (list.size() == 0) {
			return NO_FIELDS;
		}
		IField[] array= new IField[list.size()];
		list.toArray(array);
		return array;
	}
	return getFieldsOrComponents(false);
}

