	/**
	 * Advances to the next expected element
	 */
	protected void advance() {
		if (!fStack.empty()) {
			TraversalInfo info = (TraversalInfo) fStack.peek();
			fNextElement = info.next();
			if (fNextElement == null) {
				fStack.pop();
				next();
				return;
			} else {
				if (fNextElement instanceof IParent)
					push(fNextElement);
			}
		} else {
			fNextElement = null;
		}
	}

