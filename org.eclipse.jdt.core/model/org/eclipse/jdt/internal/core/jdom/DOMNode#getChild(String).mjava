/**
 * @see IDOMNode#getChild(String)
 */
public IDOMNode getChild(String name) {
	DOMNode child = fFirstChild;
	while (child != null) {
		String n = child.getName();
		if (name == null) {
			if (n == null) {
				return child;
			}
		} else {
			if (name.equals(n)) {
				return child;
			}
		}
		child = child.fNextNode;
	}
	return null;
}

