/**
 * @see IDOMField#setType(char[])
 */
public void setType(String typeName) throws IllegalArgumentException {
	if (typeName == null) {
		throw new IllegalArgumentException(Util.bind("element.nullType")); //$NON-NLS-1$
	}
	becomeDetailed();
	expand();
	fragment();
	setTypeAltered(true);
	setNameAltered(true);
	fType= typeName;
}

