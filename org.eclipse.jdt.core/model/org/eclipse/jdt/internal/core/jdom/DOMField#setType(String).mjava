	/**
	 * @see IDOMField#setType(char[])
	 */
	public void setType(String typeName) throws IllegalArgumentException {
		if (typeName == null) {
			throw new IllegalArgumentException("Illegal to set field type to null");
		}
		becomeDetailed();
		expand();
		fragment();
		setTypeAltered(true);
		setNameAltered(true);
		fType = typeName;
	}

