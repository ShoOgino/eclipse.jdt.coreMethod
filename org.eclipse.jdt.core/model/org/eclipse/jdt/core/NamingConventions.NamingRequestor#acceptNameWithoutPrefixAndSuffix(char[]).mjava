		public void acceptNameWithoutPrefixAndSuffix(char[] name) {
			int length = otherResults.length;
			if(length == otherResultsCount) {
				System.arraycopy(
				otherResults,
					0,
				otherResults = new char[length * 2][],
					0,
					length);
			}
			otherResults[otherResultsCount++] = name;
		}

