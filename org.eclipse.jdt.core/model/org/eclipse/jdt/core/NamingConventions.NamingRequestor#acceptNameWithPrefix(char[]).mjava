		public void acceptNameWithPrefix(char[] name) {
			int length = prefixResults.length;
			if(length == prefixResultsCount) {
				System.arraycopy(
					prefixResults,
					0,
					prefixResults = new char[length * 2][],
					0,
					length);
			}
			prefixResults[prefixResultsCount++] = name;
		}

