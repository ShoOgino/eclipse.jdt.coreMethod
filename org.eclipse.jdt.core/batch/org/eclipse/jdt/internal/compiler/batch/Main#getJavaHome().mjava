	private File getJavaHome() {
		if (!javaHomeChecked) {
			javaHomeChecked = true;
			String javaHome = System.getProperty("java.home");//$NON-NLS-1$
			if (javaHome != null) {
				javaHomeCache = new File(javaHome);
				if (!javaHomeCache.exists())
					javaHomeCache = null;
			}
		}
		return javaHomeCache;
	}

