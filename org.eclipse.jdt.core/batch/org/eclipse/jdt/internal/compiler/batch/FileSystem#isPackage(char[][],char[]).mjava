	public boolean isPackage(char[][] compoundName, char[] packageName) {
		if (compoundName == null)
			compoundName = new char[0][];

		for (int i = 0, length = classpaths.length; i < length; i++)
			if (classpaths[i].isPackage(compoundName, packageName))
				return true;
		return false;
	}

