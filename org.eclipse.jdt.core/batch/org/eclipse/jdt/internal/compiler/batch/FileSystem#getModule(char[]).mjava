@Override
public IModule getModule(char[] name) {
	if (name == null)
		return null;
	if (this.module != null && CharOperation.equals(name, this.module.name())) {
		return this.module;
	}
	return Stream.of(this.classpaths).map(cp -> cp.getModule(name)).filter(m -> m != null).findAny().orElse(null);
	
}

