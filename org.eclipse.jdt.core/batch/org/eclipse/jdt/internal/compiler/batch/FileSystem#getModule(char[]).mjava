@Override
public IModule getModule(char[] name) {
	if (name == null)
		return null;
	if (this.module != null && CharOperation.equals(name, this.module.name())) {
		return this.module;
	}
	IModule mod = null;
	for (int i = 0, l = this.classpaths.length; i < l; i++) {
		if ((mod = this.classpaths[i].getModule(name)) != null)
			break;
	}
	return mod;
}

